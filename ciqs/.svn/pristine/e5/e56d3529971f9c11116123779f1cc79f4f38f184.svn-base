		function priSelf(i,o){
			if (!$("#time_"+i).html()) {
				$("#name_"+i).html(o.username);
				$("#time_"+i).html(o.createdate.substring(0,16));
			} else if (new Date(o.createdate) > new Date($("#time_"+i).html())) {
				$("#name_"+i).html(o.username);
				$("#time_"+i).html(o.createdate.substring(0,16));
			}
		}


input.datepick {
	background: #FFF url(/ciqs/static/dec/images/dpn.date.pick.gif)
		no-repeat right
}
.box-img-bg {
	background-image: url(../static/show/disc/bg.png);
	box-sizing: border-box;
	width: 1198px;
	height: 164px;
	padding: 0 200px;
	position: absolute;

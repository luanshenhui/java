						if (list && list.length > 0) {
							var html = "<table width='100%'><tr><td class='table_xqlbbj2' width='33%'>附随单据名称</td>"
									+ "<td class='table_xqlbbj2' width='33%'>附随单据上传时间</td><td class='table_xqlbbj2' width='33%'>附随单据查看</td></tr>";
							for (var i = 0; i < list.length; i++) {
								//     				  		var html="<table style='height:120px; margin:0px;width:100%'><tr><td >附随单据主键</td><td >"+list[i].uuid +"</td><td >附随单据名称</td>"+
								// 	    				  		"<td >"+list[i].attachname+"</td><td >附随单据类型编码</td><td >"+list[i].attachtypecode+"</td></tr><tr><td >附随单据中文名称</td>"+
								// 	    				  		"<td >"+list[i].attachtypename+"</td><td >附随单据上传时间</td><td >"+list[i].attachtime+"</td><td >附随单据查看</td>"+
								// 	    				  		"<td ><a href='#this' onclick='urlImgDetail(\""+list[i].attachurl+"\")'>查看</a></td></tr></table>";
								// 	    					if(list[i+1]){
								// 	    						html+="<div style='height: 10px;background:#dcdcdc;'></div>";
								// 	    					}
								html += "<tr><td>"
										+ list[i].attachname
										+ "</td><td>"
										+ list[i].attachtime
										+ "</td><td ><a href='"+list[i].attachurl+"'  target=\"_blank\">查看</a></td></tr>";
							}
							;
							html += "</table>";
							$("#interfice").append(html);
							//     				  	$("#interfice td").css("border","solid 1px #dcdcdc");
						}else{
							$("#interfice").parent().remove();
						}
					}
				});

	};

	function nowDate() {
		var dd = new Date().toLocaleDateString().replace("年", "-").replace("月",
				"-").replace("日", "");
		var ss = new Date().toLocaleTimeString().replace("上午", " ").replace(
				"下午", " ");
		return dd + " " + ss;
	}

	function toImgDetail(path) {
		url = path;
		$("#imgd1").attr("src", url);
		$("#imgd1").click();
	}

	function urlImgDetail(url) {
		$("#imgd1").attr("src", url);
		$("#imgd1").click();
	}

	function getPlace(place_number) {
		window.scroll(0, document.getElementById(place_number).offsetTop - 306);
	}
</script>

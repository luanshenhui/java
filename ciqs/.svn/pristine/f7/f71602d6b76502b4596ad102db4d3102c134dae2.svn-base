	$.fn.getHour = function(startDate, endDate, flg, b) {
		if (!endDate || !startDate) {
			return $(this).html("-");
		}
		var date1 = new Date(startDate.replace(/-/g, "/"));
		var date2;
		if (b) {
			date2 = new Date();
		} else {
			date2 = new Date(endDate.replace(/-/g, "/"));
		}
		if (!flg) {
			flg = "";
		}
		var hour = "0";
		hour = (date2.getTime() - date1.getTime()) / (1000 * 60 * 60);
		hour = hour.toFixed(2);
		if (hour) {
			$(this).html(flg + hour);
		} else {
			$(this).html("-");
		}
	};


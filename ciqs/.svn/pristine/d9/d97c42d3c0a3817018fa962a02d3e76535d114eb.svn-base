/* CSS lightbox */
#lightboxWrapper{
	position: absolute;
	width: 100%;
	left: 0;
	z-index: 100;
	top: 130px;
	visibility: visible;
}
.lightbox{
	width: 100%;
	margin: 0px auto 0px auto;
	border: 5px solid #999999;
	background-color:#fff;
}
.lightbox_close{
	float: right;
}
.lightbox table{
	height: 100%;
	width: 100%;
	border-collapse:collapse;

}
.lightbox_td{
	padding: 10px;
	border: 1px solid #013978;
}

.lightbox_th{
	padding:8px;
	line-height:15px;
	text-align: left;
	border: 1px solid #013978;
	background-color: #ECF5FF;


}
.lightbox_th span{
	float: left;
	display: block;

}
.lightbox_close{
	float: right;
	margin-top: -15px;
}
.lightbox_th a{
	float: right;
	background-color: #ECF5FF;
	border: 0px solid #013978;
}
.lightbox_td table td {
	padding: 8px;
	text-align: left;
}
.lightbox_td table th{
	padding:8px;
	text-align: right;
}
.lightbox_td table tfoot td {
	height: 30px;
	text-align:center;
}
#lightbox_align{
	text-align:center;	
}

.lightbox_data th,.lightbox_data td{
	height: 26px;
	margin: 0px;
	padding: 0px;
	background-image: none;
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: none;
}
.lightbox_align{
	padding-left:auto;
	padding-right:auto;
}

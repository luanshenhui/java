/*CSS pagination*/
.pagination{
	height:60px;
	width:1200px;
	margin-top:35px;
	margin-right:auto;
	margin-left:auto;
	text-align:center;
    line-height:25px;
	}
.pagination A {
	border:#CCC 1px solid;
	padding-right:6px;
	padding-left:6px;
	padding-bottom:2px;
	color:#999;
	margin-right:2px;
	padding-top:2px;
	text-decoration:none;
}
.pagination A:hover{
	border:#999 1px solid;
	padding-right:6px;
	padding-left:6px;
	padding-bottom:2px;
	padding-top:2px;
	margin-right:2px;
	background-color:#f0f0f0;
}
.pagination A:active{
	border:#999 1px solid;
	padding-right:6px;
	padding-left:6px;
	padding-bottom:2px;
	padding-top:2px;
	margin-right:2px;
	background-color:#f0f0f0;
}
.pagination span.current {
	border:#CCC 1px solid;
	padding-right:6px;
	padding-left:6px;
	padding-bottom:2px;
	padding-top:2px;
	margin-right:2px;
	background-color:#f0f0f0;
	font-weight:bold;
	color:#999;
}
.pagination span.disabled {
	border:#CCC 1px solid;
	padding-right:6px;
	padding-left:6px;
	padding-bottom:2px;
	padding-top:2px;
	margin-right:2px;
	color:#999;

}

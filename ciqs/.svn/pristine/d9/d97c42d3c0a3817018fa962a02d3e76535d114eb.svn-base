@charset "UTF-8";
/* CSS Document */

*{padding: 0;margin: 0;}
body{
	font-family: "微软雅黑",Microsoft YaHei,'helvetica neue',tahoma,arial,'hiragino sans gb','Simsun',sans-serif;
	font-size:14px; color:#000; background-color:#f0f2f5;
}
a{color:#39F; text-decoration:none;}
a:hover{
	color:#108cee;
}
img{
	border:none;
}
div.dpn-frame-head{
	height:50px;
	margin:0 auto;
	background-image:url(../images/title-bg.png);
	background-position:right}
.dpn-frame-head .dpn-frame-head-top {
	height: 50px;
	width: 1200px;
	margin:0 auto;
}
div.dpn-frame-head span.logo{float:left;height:24px; font-size:20px; font-weight:bold; color:#FFF;margin:10px 0px 10px 0px}
div.dpn-frame-head .tollbar{float:right;display:block;color:#FFF;margin:15px 0px 15px 0px}
div.dpn-frame-head span.jump{float:right;clear:right;margin:5px 15px}
div.dpn-frame-menu{height:45px;background-color:#1b5ea8;border-top-width:1px;border-bottom-width:1px;border-top-style:solid;border-bottom-style:solid;border-top-color:#00447f;border-bottom-color:#00447f}
div.dpn-frame-menu div.loca{width:1000px;margin:0 auto;height:45px}
div.dpn-frame-menu div.loca ul.nav{position:absolute;width:1000px;left:auto;right:auto;z-index:100;color:#FFF}
div.dpn-frame-menu div.loca ul.nav>li{float:left;text-align:center;font-size:14px;font-weight:bold;height:32px;padding-top:12px;text-indent:15px;padding-right:15px; list-style-type:none}
div.dpn-frame-menu div.loca ul.nav>li:hover{background-color:#f0f2f5;color:#333}
div.dpn-frame-menu div.loca ul.nav>li>a{color:#FFF;text-decoration:none}
div.dpn-frame-menu div.loca ul.nav>li:hover>a{color:#333;text-decoration:none}
div.dpn-frame-menu div.loca ul.nav li:hover ul,div.dpn-frame-menu div.loca ul.nav li.over ul{display:block}
div.dpn-frame-menu div.loca ul.nav li ul{padding:0;margin:0;list-style:none;display:none;position:absolute;top:45px;width:auto;background-color:#FFF;background-position:left top;background-repeat:no-repeat;border-right-width:1px;border-bottom-width:1px;border-left-width:1px;border-right-style:solid;border-bottom-style:solid;border-left-style:solid;border-right-color:#00447f;border-bottom-color:#00447f;border-left-color:#00447f}
div.dpn-frame-menu div.loca ul.nav li ul li{display:block;float:none;text-align:left;clear:left;width:auto;font-size:12px;font-weight:normal}
div.dpn-frame-menu div.loca ul.nav li ul li a{display:block;background:#FFF;padding-right:15px;padding-top:6px;padding-bottom:6px;color:#333;border-top-width:1px;border-top-style:dotted;border-top-color:#9fc7f8;text-decoration:none}
div.dpn-frame-menu div.loca ul.nav li ul li a:hover{color:#fc6621;background:#c5dffe}
div.dpn-frame-content{margin:0;text-align:center;position:absolute;width:100%;left:0;right:0;height:auto;top:100px;bottom:25px;z-index:10}
div.dpn-frame-foot{position:absolute;padding-top:4px; padding-bottom:4px;height:16px;bottom:0;width:100%;left:0;right:0;text-align:center;border-top-width:1px;border-top-style:solid;border-top-color:#CCC}

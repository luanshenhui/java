			<![CDATA[ and t.apply_date >= to_date(#{apply_date_begin}, 'yyyy-mm-dd') ]]>
		</if>
		<if test="apply_date_over != null and apply_date_over != ''">
			<![CDATA[ and t.apply_date <= to_date(#{apply_date_over}, 'yyyy-mm-dd') ]]>
		</if>
		order by t.apply_date desc
	</select>
	

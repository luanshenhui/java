<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SQL.ORIGPLACE">


	<!-- 分页查询列表 -->
	<select id="findorigList"  parameterType="com.dpn.ciqqlc.standard.model.OrigPlaceDto"  resultType="com.dpn.ciqqlc.standard.model.OrigPlaceDto">
		select * from (
		select a.*,rownum rid from 
		(select 
			t.id,
			t.main_id,
			t.dec_org_name,
			t.dec_org_ename,
			t.dec_org_code,
	t.org_reg_no,
	(select CNNAME from country c where c.num_code=t.dest_country) as dest_country,
	t.transfer_country,
	t.trade_mode,
	t.shipping_date,
	t.cag_seq,
	t.hs_code,
	t.cag_name,
	t.cag_imp_comp,
	t.prod_comp,
	t.num_weight,
	t.fob_val,
	t.consignee_cname,
	t.cert_no,
	(select name from qlc_code_library qcl where t.cert_type=qcl.code and qcl.type='QLC_ZSLX') as cert_type,
	(select name from QLC_CODE_LIBRARY qcl where qcl.code=t.org_code and qcl.type='QLC_ORGANIZES') as org_code,
	(select name from QLC_CODE_LIBRARY qcl where qcl.code=t.dept_code  and qcl.type='QLC_DEPTMENTS') as dept_code,
	t.apply_date,
		t.receipt_no,
		t.receipt_date,
		t.mat_no,
		t.num_cag_disc,
		t.pric_item,
		t.cag_pric,
		t.spec_item,
		t.exp_comp,
		t.trans_type,
		(select ENNAME from country c where c.num_code=t.purpose_country) as purpose_country,
		t.pack_num,
		t.orig_place_std,
		t.m_weight,
		t.apply_fob_val,
		t.exp_stat,
		t.visa_stat,
		t.rmk,
	 	t.file_date,
		t.fiel_psn,
		t.cert_file,
		t.fob_sum,
		t.lc_number,
		t.contract_number,
		t.contact_person,
		(select ENNAME from country c where c.num_code=t.from_country) as from_country,
		t.purpose_address,
		t.create_date
		 from QLC_ORIG_PLACE t
		 where 1=1
		<if test="dec_org_name != null and dec_org_name != ''" >
			and t.dec_org_name like '%'||#{dec_org_name}||'%'
		</if>
		<if test="apply_date_begin != null and apply_date_begin != ''">
			<![CDATA[ and t.apply_date >= to_date(#{apply_date_begin}, 'yyyy-mm-dd') ]]>
		</if>
		<if test="apply_date_over != null and apply_date_over != ''">
			<![CDATA[ and t.apply_date <= to_date(#{apply_date_over}, 'yyyy-mm-dd') ]]>
		</if>
		<if test="org_reg_no != null and org_reg_no != ''" >
			and t.org_reg_no like '%'||#{org_reg_no}||'%'
		</if>
		<if test="cert_no != null and cert_no != ''" >
			and t.cert_no like '%'||#{cert_no}||'%'
		</if>
		<if test="cert_type != null and cert_type != ''" >
			and t.cert_type=#{cert_type}
		</if>
		<if test="dest_country != null and dest_country != ''" >
			and t.dest_country=#{dest_country}
		</if>
		<if test="org_code != null and org_code != ''" >
			and t.org_code=#{org_code}
		</if>
		<if test="dept_code != null and dept_code != ''" >
			and t.dept_code=#{dept_code}
		</if>
		order by APPLY_DATE desc
		 ) a 
     	where  rownum &lt;#{lastRcd}) where rid>=#{firstRcd}
	</select>
	
	<!-- 查询列表记录数 -->
	<select id="findOrigCount"  parameterType="com.dpn.ciqqlc.standard.model.OrigPlaceDto"  resultType="java.lang.Integer">
		select count(t.id) 
		 	from QLC_ORIG_PLACE t
		   	where 1=1
		<if test="dec_org_name != null and dec_org_name != ''" >
			and t.dec_org_name=#{dec_org_name}
		</if>
		<if test="apply_date_begin != null and apply_date_begin != ''">
			<![CDATA[ and t.apply_date >= to_date(#{apply_date_begin}, 'yyyy-mm-dd') ]]>
		</if>
		<if test="apply_date_over != null and apply_date_over != ''">
			<![CDATA[ and t.apply_date <= to_date(#{apply_date_over}, 'yyyy-mm-dd') ]]>
		</if>
		<if test="org_reg_no != null and org_reg_no != ''" >
			and t.org_reg_no like '%'||#{org_reg_no}||'%'
		</if>
		<if test="cert_no != null and cert_no != ''" >
			and t.cert_no like '%'||#{cert_no}||'%'
		</if>
		<if test="cert_type != null and cert_type != ''" >
			and t.cert_type=#{cert_type}
		</if>
		<if test="dest_country != null and dest_country != ''" >
			and t.dest_country=#{dest_country}
		</if>
		<if test="org_code != null and org_code != ''" >
			and t.org_code=#{org_code}
		</if>
		<if test="dept_code != null and dept_code != ''" >
			and t.dept_code=#{dept_code}
		</if>
	</select>


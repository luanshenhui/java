	<select id="getViewImg"  parameterType="com.dpn.ciqqlc.standard.model.VideoEventModel"  resultType="com.dpn.ciqqlc.standard.model.VideoEventModel">
	SELECT
		id,
		proc_Main_Id as procMainId,
		proc_Type as procType,
		file_Type as fileType,
		file_Name as fileName,
		port_Dept_Code as portDeptCode,
		port_Org_Code as portOrgCode,
		create_User as createUser,
		name,
		create_Date as createDate
		FROM
		qlc_video_file_event t
		where 1=1
		<if test="procMainId != null and procMainId != ''" >
		and proc_main_id=#{procMainId}
		</if>
		<if test="procType != null and procType != ''" >
		and proc_type=#{procType}
		</if>
		<if test="proj_code != null and proj_code != ''" >
		 AND EXISTS (
	              SELECT 
	                        PROC_TYPE
	              FROM 
	                        QLC_C_VIDEO_PROC_TYPE
	              WHERE   
	                        PROJ_CODE=#{proj_code}
	              AND 
	                        PROC_TYPE=t.PROC_TYPE
    	)   
    	</if>  
    	order by  create_Date desc          
	</select>
	
	

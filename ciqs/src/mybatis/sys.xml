    <select id="selectDt" resultType="java.util.Date">
        <![CDATA[
        SELECT 
            SYSDATE 
        FROM 
            DUAL
        ]]>
    </select>

    <select id="selectGuid" resultType="string">
        <![CDATA[
        SELECT 
            SYS_GUID() 
        FROM 
            DUAL
        ]]>

	var arr =new Array();
	//国境口岸餐饮服务单位卫生许可现场审查表（二）
	var s1={p:1,a:"选址",i:"1",b:"1.选择地势干燥、有给排水条件和电力供应的地区，不得设在易受到污染的区域。距离粪坑、污水池、暴露垃圾场（站）、旱厕等污染源25m以上，并设置在粉尘、有害气体、放射性物质和其他扩散性污染源的影响范围之外。",c:"※"};
	
	var s2={p:9,a:"场所设置、布局、分隔和面积(19分)",i:"2",b:"设置与食品供应方式和品种相适应的粗加工、切配、烹饪、面点制作、餐用具清洗消毒、备餐等加工操作场所，以及食品库房、更衣室、清洁工具存放场所等。各场所均设在室内。",c:"※"};
	var s3={a:"场所设置、布局、分隔和面积(19分)",i:"3",b:"进行凉菜配制、裱花操作和食堂备餐，分别设置相应操作专间。",c:"※"};
	var s4={a:"场所设置、布局、分隔和面积(19分)",i:"4",b:"制作现榨果蔬汁和水果拼盘及加工生食海产品，设置相应的专用操作场所",c:"5"};
	var s5={a:"场所设置、布局、分隔和面积(19分)",i:"5",b:"各加工操作场所按照原料进入、原料处理、半成品加工、成品供应的顺序合理布局，能防止食品在存放、操作中产生交叉污染。",c:"5"};
	var s6={a:"场所设置、布局、分隔和面积(19分)",i:"6",b:"用于原料、半成品、成品的工具、用具和容器，有明显的区分标识，存放区域分开设置。",c:"※"};
	var s7={a:"场所设置、布局、分隔和面积(19分)",i:"7",b:"食品处理区面积与就餐场所面积之比≥1:2.2 。",c:"5"};
	var s8={a:"场所设置、布局、分隔和面积(19分)",i:"8",b:"切配烹饪场所面积≥食品处理区面积50%（全部用半成品烹饪的可适当减少），凉菜间面积≥食品处理区面积10%。",c:"2"};
	var s9={a:"场所设置、布局、分隔和面积(19分)",i:"9",b:"凉菜间面积≥5㎡。",c:"※"};
	var s10={a:"场所设置、布局、分隔和面积(19分)",i:"10",b:"加工经营场所内无圈养、宰杀活的禽畜类动物的区域（或距离25m以上）。",c:"2"};

	var s11={p:7,a:"食品处理区地面、墙壁、门窗、天花板与排水(23分)",i:"11",b:"地面用无毒、无异味、不透水、不易积垢、耐腐蚀、防滑的材料铺设，且平整、无裂缝。粗加工、切配、餐用具清洗消毒和烹调等场所的地面易于清洗、防滑，并有排水系统。",c:"5"};
	var s12={a:"食品处理区地面、墙壁、门窗、天花板与排水(23分)",i:"12",b:"地面和排水沟有排水坡度。",c:"2"};
	var s13={a:"食品处理区地面、墙壁、门窗、天花板与排水(23分)",i:"13",b:"排水沟出口有网眼孔径小于6mm的金属隔栅或网罩。",c:"2"};
	var s14={a:"食品处理区地面、墙壁、门窗、天花板与排水(23分)",i:"14",b:"墙壁采用无毒、无异味、不透水、平滑、不易积垢的浅色材料，粗加工、切配、餐用具清洗消毒和烹调等场所有1.5m以上光滑、不吸水、浅色、耐用和易清洗的材料制成的墙裙。",c:"5"};
	var s15={a:"食品处理区地面、墙壁、门窗、天花板与排水(23分)",i:"15",b:"门、窗装配严密，与外界直接相通的门和可开启的窗设有易于拆洗且不生锈的防蝇纱网或设置空气幕，与外界直接相通的门能自动关闭。",c:"2"};
	var s16={a:"食品处理区地面、墙壁、门窗、天花板与排水(23分)",i:"16",b:"粗加工、切配、餐用具清洗消毒、烹调等场所如设门，采用易清洗、不吸水的坚固材料制作。",c:"2"};
	var s17={a:"食品处理区地面、墙壁、门窗、天花板与排水(23分)",i:"17",b:"天花板采用无毒、无异味、不吸水、表面光洁、耐腐蚀、耐温、浅色材料涂覆或装修。",c:"5"};
	
	var s18={p:2,a:"洗手消毒设施(7分)",i:"18",b:"食品处理区内设置足够数量的洗手设施，其位置设置在方便员工的区域。",c:"5"};
	var s19={a:"洗手消毒设施(7分)",i:"19",b:"洗手消毒设施附近有相应的清洗、消毒用品和干手用品或设施，员工专用洗手消毒设施附近有洗手消毒方法标识。",c:"2"};
	
	var s20={p:5,a:"餐用具清洗消毒保洁设施(7分)",i:"20",b:"配备能正常运转的清洗、消毒、保洁设备设施。",c:"※"};
	var s21={a:"餐用具清洗消毒保洁设施(7分)",i:"21",b:"各类清洗消毒方式设专用水池的最低数量：采用化学消毒的，至少设有3个专用水池。采用人工清洗热力消毒的，可设置２个专用水池。各类水池以明显标识标明其用途。",c:"※"};
	var s22={a:"餐用具清洗消毒保洁设施(7分)",i:"22",b:"餐用具清洗消毒水池专用，与食品原料、清洁用具及接触非直接入口食品的工具、容器清洗水池分开。",c:"※"};
	var s23={a:"餐用具清洗消毒保洁设施(7分)",i:"23",b:"设专供存放消毒后餐用具的保洁设施，标记明显，结构密闭并易于清洁。",c:"2"};
	var s24={a:"餐用具清洗消毒保洁设施(7分)",i:"24",b:"清洗、消毒、保洁设备设施的大小和数量能满足需要。",c:"5"};
	
	var s25={p:2,a:"食品原料、清洁工具清洗水池(7分)",i:"25",b:"粗加工操作场所分别设动物性食品、植物性食品、水产品3类食品原料的清洗水池，水池数量或容量与加工食品的数量相适应。各类水池以明显标识标明其用途。",c:"5"};
	var s26={a:"食品原料、清洁工具清洗水池(7分)",i:"26",b:"设专用于拖把等清洁工具、用具的清洗水池，其位置不会污染食品及其加工制作过程。",c:"2"};
	
	var s27={p:2,a:"设备、工具和容器(7分)",i:"27",b:"接触食品的设备、工具、容器、包装材料等符合食品安全标准或要求。",c:"5"};
	var s28={a:"设备、工具和容器(7分)",i:"28",b:"接触食品的设备、工具和容器易于清洗消毒。",c:"2"};
	
	var s29={p:2,a:"通风排烟设施(7分)",i:"29",b:"烹调场所采用机械排风。产生油烟的设备上部加设附有机械排风及油烟过滤的排气装置，过滤器便于清洗和更换。",c:"5"};
	var s30={a:"通风排烟设施(7分)",i:"30",b:"排气口装有网眼孔径小于6mm的金属隔栅或网罩。",c:"2"};
	
	var s31={p:2,a:"废弃物暂存设施(7分)",i:"31",b:"食品处理区设存放废弃物或垃圾的容器。废弃物容器与加工用容器有明显区分的标识。",c:"5"};
	var s32={a:"废弃物暂存设施(7分)",i:"32",b:"废弃物容器配有盖子，以坚固及不透水的材料制造，内壁光滑便于清洗。专间内的废弃物容器盖子为非手动开启式。",c:"2"};
	
	var s33={p:2,a:"库房和食品贮存场所(5分)",i:"33",b:"食品和非食品（不会导致食品污染的食品容器、包装材料、工具等物品除外）库房分开设置。",c:"5"};
	var s34={a:"库房和食品贮存场所(5分)",i:"34",b:"冷藏、冷冻柜（库）数量和结构能使原料、半成品和成品分开存放，有明显区分标识。",c:"※"};
	
	var s35={p:4,a:"专间(10分)",i:"35",b:"专间内无明沟，地漏带水封。食品传递窗为开闭式，其他窗封闭。专间墙裙铺设到顶。",c:"5"};
	var s36={a:"专间(10分)",i:"36",b:"专间门采用易清洗、不吸水的坚固材质，能够自动关闭。",c:"5"};
	var s37={a:"专间(10分)",i:"37",b:"专间内设独立空调设施、专用工具清洗消毒设施和空气消毒设施以及专用冷藏设施。",c:"※"};
	var s38={a:"专间(10分)",i:"38",b:"专间入口处设置有非手动式洗手、消毒、更衣设施的通过式预进间。",c:"※"};
	
	var s39={p:2,a:"厕所(2分)",i:"39",b:"厕所不设在食品处理区。",c:"※"};
	var s40={a:"厕所(2分)",i:"40",b:"厕所采用水冲式，在出口附近设置洗手设施。",c:"2"};
	

	var e3={a:"场所设置、布局、分隔和面积（10分）",i:"3",b:"各加工操作场所按照原料进入、原料处理、半成品加工、成品供应的顺序合理布局，并能防止食品在存放、操作中产生交叉污染。",c:"5"};
	var e4={a:"场所设置、布局、分隔和面积（10分）",i:"4",b:"用于原料、半成品、成品的工具、用具和容器，有明显的区分标识，存放区域分开设置",c:"※"};
	var e5={a:"场所设置、布局、分隔和面积(10分)",i:"5",b:"食品处理区面积与就餐场所面积之比，以及最大供餐人数符合要求。",c:"5"};
	
	var e6={p:4,a:"食品处理区地面墙壁、天花板、门窗与排水（8分）",i:"6",b:"地面用无毒、无异味、不透水、不易积垢、耐腐蚀、防滑的材料铺设，且平整、无裂缝。粗加工、切配、餐用具清洗消毒和烹调等场所的地面易于清洗、防滑，并有排水系统。",c:"2"};
	var e7={a:"食品处理区地面墙壁、天花板、门窗与排水（8分）",i:"7",b:"墙壁采用无毒、无异味、不透水、平滑、不易积垢的浅色材料，粗加工、切配、餐用具清洗消毒和烹调等场所有1.5m以上光滑、不吸水、浅色、耐用和易清洗的材料制成的墙裙 。",c:"2"};
	var e8={a:"食品处理区地面墙壁、天花板、门窗与排水（8分）",i:"8",b:"门、窗装配严密，与外界直接相通的门和可开启的窗设有易于拆洗且不生锈的防蝇纱门(窗)或设置空气幕，与外界直接相通的门能自动关闭。",c:"2"};
	var e9={a:"食品处理区地面墙壁、天花板、门窗与排水（8分）",i:"9",b:"天花板不渗水，无脱落、破损。",c:"2"};
	
	var e10={p:2,a:"餐用具清洗消毒保洁设施",i:"10",b:"各类清洗消毒方式设专用水池的最低数量：采用化学消毒的，至少设有3个专用水池。采用人工清洗热力消毒的，可设置２个专用水池。各类水池以明显标识标明其用途。",c:"※"};
	var e11={a:"餐用具清洗消毒保洁设施",i:"11",b:"配备能正常运转的清洗、消毒、保洁设备设施，设专供存放消毒后餐用具的保洁设施，标记明显，结构密闭并易于清洁。清洗、消毒、保洁设备设施的大小和数量能满足需要",c:"※"};

	var e12={p:2,a:"食品原料、清洁工具清洗水池（7分）",i:"12",b:"粗加工操作场所分别设动物性食品、植物性食品、水产品3类食品原料的清洗水池，水池数量或容量与加工食品的数量相适应。各类水池以明显标识标明其用途。",c:"5"};
	var e13={a:"食品原料、清洁工具清洗水池（7分）",i:"13",b:"设专用于拖把等清洁工具、用具的清洗水池，其位置不会污染食品及其加工制作过程。",c:"2"};
	
	var e14={p:1,a:"设备、工具和容器（5分）",i:"14",b:"接触食品的设备、工具、容器、包装材料等符合食品安全标准或要求。",c:"5"};
	
	var e15={p:1,a:"通风排烟设施（5分）",i:"15",b:"烹调场所采用机械排风。产生油烟的设备上部加设附有机械排风及油烟过滤的排气装置，过滤器便于清洗和更换。",c:"5"};
	
	var e16={p:2,a:"废弃物暂存设施（7分）",i:"16",b:"食品处理区设存放废弃物或垃圾的容器。废弃物容器与加工用容器有明显区分的标识。",c:"5"};
	var e17={a:"废弃物暂存设施（7分）",i:"17",b:"废弃物容器配有盖子，以坚固及不透水的材料制造，内壁光滑便于清洗。专间内的废弃物容器盖子为非手动开启式。",c:"2"};
	
	var e18={p:2,a:"库房和食品贮存场所（7分）",i:"18",b:"食品和非食品（不会导致食品污染的食品容器、包装材料、工具等物品除外）库房分开设置。",c:"5"};
	var e19={a:"库房和食品贮存场所（7分）",i:"19",b:"冷藏、冷冻柜（库）数量和结构能使原料、半成品和成品分开存放，有明显区分标识。",c:"2"};
	
	var e20={p:5,a:"备餐专间（10分）",i:"20",b:"专间内无明沟，地漏带水封。食品传递窗为开闭式，其他窗封闭。专间墙裙铺设到顶",c:"5"};
	var e21={a:"备餐专间（10分）",i:"21",b:"专间门采用易清洗、不吸水的坚固材质，能够自动关闭。",c:"5"};
	var e22={a:"备餐专间（10分）",i:"22",b:"专间内设独立空调设施、专用工具清洗消毒设施和空气消毒设施以及专用冷藏设施。",c:""};
	var e23={a:"备餐专间（10分）",i:"23",b:"专间入口处设置洗手、消毒、更衣设施。",c:"※"};
	var e24={a:"备餐专间（10分）",i:"24",b:"具备充足的保温和运输设备设施。",c:"※"};
	//国境口岸餐饮服务单位卫生许可现场检查表（五）
	var f1={p:1,a:"选址(15分)",i:"1",b:"选择地势干燥、有给排水条件和电力供应的地区，不得设在易受到污染的区域。距离粪坑、污水池、暴露垃圾场（站）、旱厕等污染源25m以上，并设置在粉尘、有害气体、放射性物质和其他扩散性污染源的影响范围之外。",c:"※"};
	
	var f2={p:7,a:"场所设置、布局、分隔和面积（11分）",i:"2",b:"设置与食品供应方式和品种相适应的粗加工、切配、烹饪、面点制作、餐用具清洗消毒、备餐等加工操作场所，以及食品库房、更衣室、清洁工具存放场所等。各场所均设在室内。",c:"※"};
	var f3={a:"场所设置、布局、分隔和面积（11分）",i:"3",b:"各加工操作场所按照原料进入、原料处理、半成品加工、成品供应的顺序合理布局，并能防止食品在存放、操作中产生交叉污染。",c:"5"};
	var f4={a:"场所设置、布局、分隔和面积（11分）",i:"4",b:"用于原料、半成品、成品的工具、用具和容器，有明显的区分标识，存放区域分开设置。",c:"※"};
	var f5={a:"场所设置、布局、分隔和面积（11分）",i:"5",b:"食品处理区面积与最大供餐人数相适应，小于200㎡，面积与单班最大生产份数之比为1:2.5；200—400㎡，面积与单班最大生产份数之比为1:2.5；400—800㎡，面积与单班最大生产份数之比为1:4；800-1500㎡，面积与单班最大生产份数之比为1:6；面积大于1500㎡的，其面积与单班最大生产份数之比可适当减少。",c:"※"};
	var f6={a:"场所设置、布局、分隔和面积（11分）",i:"6",b:"烹饪场所面积≥食品处理区面积15%，分餐间面积≥食品处理区10%，清洗消毒面积≥食品处理区10%。",c:"2"};
	var f7={a:"场所设置、布局、分隔和面积（11分）",i:"7",b:"厂区道路采用混凝土、沥青等便于清洗的硬质材料铺设，有良好的排水系统。",c:"2"};
	var f8={a:"场所设置、布局、分隔和面积（11分）",i:"8",b:"加工经营场所内无圈养、宰杀活的禽畜类动物的区域（或距离25m以上）",c:"2"};
	
	var f9={p:4,a:"食品处理区地面与排水（12分）",i:"9",b:"地面用无毒、无异味、不透水、不易积垢、耐腐蚀、防滑的材料铺设，且平整、无裂缝。粗加工、切配、餐用具清洗消毒和烹调等场所的地面易于清洗、防滑，并有排水系统。",c:"※"};
	var f10={a:"食品处理区地面与排水（12分）",i:"10",b:"墙角、柱角、侧面、底面的结合处有一定的弧度。",c:"2"};
	var f11={a:"食品处理区地面与排水（12分）",i:"11",b:"地面和排水沟有排水坡度。",c:"5"};
	var f12={a:"食品处理区地面与排水（12分）",i:"12",b:"排水沟出口有网眼孔径小于6mm的金属隔栅或网罩。",c:"5"};
	
	var f13={p:4,a:"食品处理区墙壁、门窗（17分）",i:"13",b:"墙壁采用无毒、无异味、不透水、平滑、不易积垢的浅色材料，粗加工、切配、餐用具清洗消毒和烹调等场所有1.5m以上光滑、不吸水、浅色、耐用和易清洗的材料制成的墙裙。",c:"5"};
	var f14={a:"食品处理区墙壁、门窗（17分）",i:"14",b:"门、窗装配严密，与外界直接相通的门和可开启的窗设有易于拆洗且不生锈的防蝇纱网或设置空气幕，与外界直接相通的门能自动关闭。",c:"5"};
	var f15={a:"食品处理区墙壁、门窗（17分）",i:"15",b:"粗加工、切配、餐用具清洗消毒、烹调等场所如设门，采用易清洗、不吸水的坚固材料制作。",c:"5"};
	var f16={a:"食品处理区墙壁、门窗（17分）",i:"16",b:"内窗台下斜45度或采用无窗台结构。",c:"2"};
	
	var f17={p:3,a:"食品处理区天花板",i:"17",b:"天花板采用无毒、无异味、不吸水、表面光洁、耐腐蚀、耐温、浅色材料涂覆或装修。",c:"5"};
	var f18={a:"食品处理区天花板",i:"18",b:"清洁操作区、准清洁操作区及其它半成品、成品暴露场所屋顶若为不平整的结构或有管道通过，加设平整、易于清洁的吊顶。",c:"5"};
	var f19={a:"食品处理区天花板",i:"19",b:"水蒸气较多的场所的天花板有适当的坡度。",c:"※"};
	
	var f20={p:3,a:"洗手消毒设施（9分）",i:"20",b:"食品处理区内设置足够数量的洗手设施，其位置设置在方便员工的区域。",c:"5"};
	var f21={a:"洗手消毒设施（9分）",i:"21",b:"洗手池的材质为不透水材料，结构易于清洗",c:"2"};
	var f22={a:"洗手消毒设施（9分）",i:"22",b:"洗手消毒设施附近有相应的清洗、消毒用品和干手用品或设施，员工专用洗手消毒设施附近有洗手消毒方法标识。",c:"2"};
	
	var f23={p:7,a:"餐用具清洗消毒保洁设施（12分）",i:"23",b:"配备能正常运转的清洗、消毒、保洁设备设施。",c:"※"};
	var f24={a:"餐用具清洗消毒保洁设施（12分）",i:"24",b:"采用热力消毒（因材质等原因无法采用的除外）。",c:"5"};
	var f25={a:"餐用具清洗消毒保洁设施（12分）",i:"25",b:"各类清洗消毒方式设专用水池的最低数量：采用化学消毒的，至少设有3个专用水池。采用人工清洗热力消毒的，可设置２个专用水池。各类水池以明显标识标明其用途。",c:"※"};
	var f26={a:"餐用具清洗消毒保洁设施（12分）",i:"26",b:"餐用具清洗消毒水池专用，与食品原料、清洁用具及接触非直接入口食品的工具、容器清洗水池分开",c:"※"};
	var f27={a:"餐用具清洗消毒保洁设施（12分）",i:"27",b:"餐用具清洗消毒水池使用不锈钢或陶瓷等不透水材料、不易积垢并易于清洗。",c:"2"};
	var f28={a:"餐用具清洗消毒保洁设施（12分）",i:"28",b:"设专供存放消毒后餐用具的保洁设施，标记明显，结构密闭并易于清洁。",c:"5"};
	var f29={a:"餐用具清洗消毒保洁设施（12分）",i:"29",b:"清洗、消毒、保洁设备设施的大小和数量能满足需要",c:"※"};

	var f30={p:2,a:"食品原料、清洁工具清洗水池（5分）",i:"30",b:"粗加工操作场所分别设动物性食品、植物性食品、水产品3类食品原料的清洗水池，水池数量或容量与加工食品的数量相适应。各类水池以明显标识标明其用途。",c:"5"};
	var f31={a:"食品原料、清洁工具清洗水池（5分）",i:"31",b:"设专用于拖把等清洁工具、用具的清洗水池，其位置不会污染食品及其加工制作过程",c:"※"};
	
	var f32={p:4,a:"设备、工具和容器（14分）",i:"32",b:"接触食品的设备、工具、容器、包装材料等符合食品安全标准或要求。",c:"5"};
	var f33={a:"设备、工具和容器（14分）",i:"33",b:"接触食品的设备、工具和容器易于清洗消毒",c:"2"};
	var f34={a:"设备、工具和容器（14分）",i:"34",b:"食品设备、工具和容器与食品的接触面平滑、无凹陷或裂缝",c:"5"};
	var f35={a:"设备、工具和容器（14分）",i:"35",b:"所有食品设备、工具和容器不使用木质材料，因工艺要求必须使用除外。",c:"2"};
	
	var f36={p:2,a:"通风排烟设施（10分)",i:"36",b:"烹调场所采用机械排风。产生油烟的设备上部应加设附有机械排风及油烟过滤的排气装置，过滤器应便于清洗和更换。",c:"5"};
	var f37={a:"通风排烟设施（10分)",i:"37",b:"排气口装有网眼孔径小于6mm的金属隔栅或网罩。",c:"5"};
	
	var f38={p:1,a:"采光照明设施（2分）",i:"38",b:"加工经营场所光源不改变所观察食品的天然颜色。安装在暴露食品正上方的照明设施使用防护罩。冷冻（藏）库房使用防爆灯。",c:"2"};
	
	var f39={p:2,a:"废弃物暂存设施（4分）",i:"39",b:"食品处理区设存放废弃物或垃圾的容器。废弃物容器与加工用容器有明显区分的标识。",c:"2"};
	var f40={a:"废弃物暂存设施（4分）",i:"40",b:"废弃物容器配有盖子，以坚固及不透水的材料制造，内壁光滑便于清洗。专间内的废弃物容器盖子为非手动开启式。",c:"2"};
	
	var f41={p:4,a:"库房和食品贮存场所（9分）",i:"41",b:"食品和非食品（不会导致食品污染的食品容器、包装材料、工具等物品除外）库房分开设置。",c:"5"};
	var f42={a:"库房和食品贮存场所（9分）",i:"42",b:"冷藏、冷冻柜（库）数量和结构应能使原料、半成品和成品分开存放，有明显区分标识。",c:"※"};
	var f43={a:"库房和食品贮存场所（9分）",i:"43",b:"除冷库外的库房有良好的通风、防潮、防鼠（如设防鼠板或木质门下方以金属包覆）设施。",c:"2"};
	var f44={a:"库房和食品贮存场所（9分）",i:"44",b:"冷冻（藏）库设可正确指示库内温度的温度计。",c:"2"};
	
	var f45={p:4,a:"专间(10分)",i:"45",b:"专间内无明沟，地漏带水封。食品传递窗为开闭式，其他窗封闭。专间墙裙铺设到顶。",c:"5"};
	var f46={a:"专间(10分)",i:"46",b:"专间门采用易清洗、不吸水的坚固材质，能够自动关闭。",c:"5"};
	var f47={a:"专间(10分)",i:"47",b:"专间内设独立空调设施、专用工具清洗消毒设施和空气消毒设施以及专用冷藏设施。",c:"※"};
	var f48={a:"专间(10分)",i:"48",b:"专间入口处设置有洗手、消毒、更衣设施的通过式预进间，洗手消毒设施符合本表第6项的规定。",c:"※"};
	
	var f49={p:1,a:"更衣室(2分)",i:"49",b:"更衣场所与加工经营场所处于同一建筑物内，有足够大小的空间、足够数量的更衣设施和适当的照明。",c:"2"};
	
	var f50={p:2,a:"厕所(4分)",i:"50",b:"厕所不设在食品处理区。",c:"2"};
	var f51={a:"厕所(4分)",i:"51",b:"厕所采用水冲式，在出口附近设置洗手设施。",c:"2"};
	
	var f52={p:2,a:"运输设备(2分)",i:"52",b:"配备封闭式专用运输车辆，以及专用密闭运输容器。",c:"※"};
	var f53={a:"运输设备(2分)",i:"53",b:"运输车辆和容器内部材质和结构便于清洗和消毒。",c:"2"};
	
	var f54={p:1,a:"留样（5分）",i:"54",b:"配备专用留样容器和冷藏设施。",c:"5"};
	
	//国境口岸餐饮服务单位卫生许可现场检查表（六）
	var h1={p:1,a:"选址",i:"1",b:"选择地势干燥、有给排水条件和电力供应的地区，不得设在易受到污染的区域。距离粪坑、污水池、暴露垃圾场（站）、旱厕等污染源25m以上，并设置在粉尘、有害气体、放射性物质和其他扩散性污染源的影响范围之外。",c:"※"};
	
	var h2={p:10,a:"场所设置、布局、分隔、面积（4分）",i:"2",b:"设置具有与供应品种、数量相适应的粗加工、切配、烹调、面点制作、食品冷却、食品包装、待配送食品贮存、工用具清洗消毒等加工操作场所，以及食品库房、更衣室、清洁工具存放场所等。食品处理区分为一般操作区、准清洁区、清洁区，各食品处理区均应设置在室内，且独立分隔。",c:"※"};
	var h3={a:"场所设置、布局、分隔、面积（4分）",i:"3",b:"配制凉菜以及待配送食品贮存的，应分别设置食品加工专间；食品冷却、包装应设置食品加工专间或专用设施。",c:"※"};
	var h4={a:"场所设置、布局、分隔、面积（4分）",i:"4",b:"各加工操作场所按照原料进入、原料处理、半成品加工、食品分装及待配送食品贮存的顺序合理布局，并能防止食品在存放、操作中产生交叉污染。",c:"※"};
	var h5={a:"场所设置、布局、分隔、面积（4分）",i:"5",b:"接触原料、半成品、成品的工具、用具和容器，有明显的区分标识，且分区域存放；接触动物性和植物性食品的工具、用具和容器也有明显的区分标识，且分区域存放",c:"※"};
	var h6={a:"场所设置、布局、分隔、面积（4分）",i:"6",b:"食品加工操作和贮存场所面积原则上≥300㎡，应当与加工食品的品种和数量相适应。",c:"※"};
	var h7={a:"场所设置、布局、分隔、面积（4分）",i:"7",b:"切配烹饪场所面积≥食品处理区面积15%",c:"※"};
	var h8={a:"场所设置、布局、分隔、面积（4分）",i:"8",b:"清洗消毒区面积≥食品处理区面积10%",c:"※"};
	var h9={a:"场所设置、布局、分隔、面积（4分）",i:"9",b:"凉菜专间面积≥10 ㎡",c:"※"};
	var h10={a:"场所设置、布局、分隔、面积（4分）",i:"10",b:"厂区道路采用混凝土、沥青等便于清洗的硬质材料铺设，有良好的排水系统",c:"2"};
	var h11={a:"场所设置、布局、分隔、面积（4分）",i:"11",b:"加工经营场所内无圈养、宰杀活的禽畜类动物的区域（或距离25m以上）",c:"2"};
	
	var h12={p:3,a:"食品处理区地面、排水（10分）",i:"12",b:"地面用无毒、无异味、不透水、不易积垢的材料铺设，且平整、无裂缝。粗加工、切配、加工用具清洗消毒和烹调等需经常冲洗场所、易潮湿场所的地面易于清洗、防滑，并有排水系统",c:"※"};
	var h13={a:"食品处理区地面、排水（10分）",i:"13",b:"地面和排水沟有排水坡度（不小于1.5%），排水的流向由高清洁操作区流向低清洁操作区",c:"5"};
	var h14={a:"食品处理区地面、排水（10分）",i:"14",b:"排水沟出口有网眼孔径小于6mm的金属隔栅或网罩",c:"5"};
	
	var h15={p:5,a:"墙壁与门窗（22分）",i:"15",b:"墙壁采用无毒、无异味、不透水、平滑、不易积垢的浅色材料，粗加工、切配、烹调和工用具清洗消毒等场所应有1.5 m以上的光滑、不吸水、浅色、耐用和易清洗的材料制成的墙裙，食品加工专间内应铺设到顶",c:"5"};
	var h16={a:"墙壁与门窗（22分）",i:"16",b:"墙角、柱脚、侧面、底面的结合处有一定的弧度。",c:"5"};
	var h17={a:"墙壁与门窗（22分）",i:"17",b:"门、窗装配严密，与外界直接相通的门和可开启的窗设有易于拆下清洗不生锈的纱网或空气幕，与外界直接相通的门和各类专间的门能自动关闭",c:"5"};
	var h18={a:"墙壁与门窗（22分）",i:"18",b:"内窗台下斜45度以上或采用无窗台结构",c:"5"};
	var h19={a:"墙壁与门窗（22分）",i:"19",b:"粗加工、切配、烹调、工用具清洗消毒等场所、食品包装间的门采用易清洗、不吸水的坚固材料制作。",c:"2"};
	
	var h20={p:3,a:"天花板（12分）",i:"20",b:"天花板用无毒、无异味、不吸水、表面光洁、耐腐蚀、耐温、浅色材料涂覆或装修。",c:"5"};
	var h21={a:"天花板（12分）",i:"21",b:"半成品、即食食品暴露场所屋顶若为不平整的结构或有管道通过，加设平整易于清洁的吊顶（吊顶间缝隙应严密封闭）。",c:"2"};
	var h22={a:"天花板（12分）",i:"22",b:"水蒸气较多的场所的天花板有适当的坡度（斜坡或拱形均可）。",c:"5"};
	
	var h23={p:3,a:"洗手消毒设施（9分）",i:"23",b:"食品处理区内设置足够数量的洗手设施，其位置设置在方便员工的区域",c:"5"};
	var h24={a:"洗手消毒设施（9分）",i:"24",b:"洗手池的材质为不透水材料，结构不易积垢并易于清洗。",c:"2"};
	var h25={a:"洗手消毒设施（9分）",i:"25",b:"洗手消毒设施旁设有相应的清洗、消毒用品和干手设施，员工专用洗手消毒设施附近有洗手消毒方法标识。",c:"2"};
	
	var h26={p:7,a:"工用具清洗消毒保洁设施（12分）",i:"26",b:"根据加工食品的品种，配备能正常运转的清洗、消毒、保洁设备设施。",c:"※"};
	var h27={a:"工用具清洗消毒保洁设施（12分）",i:"27",b:"采用有效的物理消毒或化学消毒方法",c:"5"};
	var h28={a:"工用具清洗消毒保洁设施（12分）",i:"28",b:"各类清洗消毒方式设专用水池的最低数量：采用化学消毒的，至少设有3个专用水池或容器。采用热力消毒的，可设置２个专用水池或容器。各类水池或容器以明显标识标明其用途",c:"※"};
	var h29={a:"工用具清洗消毒保洁设施（12分）",i:"29",b:"接触直接入口食品的工具、容器清洗消毒水池专用，与食品原料、清洁用具及接触非直接入口食品的工具、容器清洗水池分开",c:"※"};
	var h30={a:"工用具清洗消毒保洁设施（12分）",i:"30",b:"工用具清洗消毒水池使用不锈钢或陶瓷等不透水材料、不易积垢并易于清洗",c:"2"};
	var h31={a:"工用具清洗消毒保洁设施（12分）",i:"31",b:"设专供存放消毒后工用具的保洁设施，标记明显，易于清洁",c:"5"};
	var h32={a:"工用具清洗消毒保洁设施（12分）",i:"32",b:"清洗、消毒、保洁设备设施的大小和数量能满足需要。",c:"※"};
	
	var h33={p:2,a:"加工食品设备、工具和容器（14分）",i:"33",b:"粗加工操作场所分别设动物性食品、植物性食品、水产品3类食品原料的清洗水池，水池数量或容量与加工食品的数量相适应。各类水池以明显标识标明其用途",c:"5"};
	var h34={a:"加工食品设备、工具和容器（14分）",i:"34",b:"加工场所内设专用于拖把等清洁工具、用具的清洗水池，其位置不会污染食品及其加工操作过程。",c:"※"};
	
	var h35={p:6,a:"加工食品设备、工具和容器（14分）",i:"35",b:"食品烹调后以冷冻（藏）方式保存的，应根据加工食品的品种和数量，配备相应数量的食品快速冷却设备。",c:"※"};
	var h36={a:"加工食品设备、工具和容器（14分）",i:"36",b:"应根据待配送食品的品种、数量、配送方式，配备相应的食品包装设备",c:"※"};
	var h37={a:"加工食品设备、工具和容器（14分）",i:"37",b:"接触食品的设备、工具、容器、包装材料等符合食品安全标准或要求",c:"5"};
	var h38={a:"加工食品设备、工具和容器（14分）",i:"38",b:"接触食品的设备、工具和容器易于清洗消毒",c:"2"};
	var h39={a:"加工食品设备、工具和容器（14分）",i:"39",b:"所有食品设备、工具和容器不使用木质材料，因工艺要求必须使用除外",c:"2"};
	var h40={a:"加工食品设备、工具和容器（14分）",i:"40",b:"食品容器、工具和设备与食品的接触面平滑、无凹陷或裂缝（因工艺要求除外）",c:"5"};
	
	var h41={p:2,a:"通风排烟设施（10分）",i:"41",b:"食品烹调场所采用机械排风。产生油烟或大量蒸汽的设备上部，加设附有机械排风及油烟过滤的排气装置，过滤器便于清洗和更换。",c:"5"};
	var h42={a:"通风排烟设施（10分）",i:"42",b:"排气口装有网眼孔径小于6mm的金属隔栅或网罩。",c:"2"};
	
	var h43={p:2,a:"采光照明设施（4分）",i:"43",b:"加工经营场所光源不改变所观察食品的天然颜色。",c:"2"};
	var h44={a:"采光照明设施（4分）",i:"44",b:"安装在食品暴露正上方的照明设施使用防护罩。冷冻（藏）库房使用防爆灯",c:"2"};

	var h45={p:2,a:"废弃物暂存设施（4分）",i:"45",b:"食品处理区设存放废弃物或垃圾的容器。废弃物容器与加工用容器有明显区分的标识。",c:"2"};
	var h46={a:"废弃物暂存设施（4分）",i:"46",b:"废弃物容器配有盖子，以坚固及不透水的材料制造，内壁光滑便于清洗。专间内的废弃物容器盖子为非手动开启式",c:"2"};

	var h47={p:5,a:"库房和食品贮存场所（11分）",i:"47",b:"食品和非食品（不会导致食品污染的食品容器、包装材料、工用具等物品除外）库房分开设置。",c:"5"};
	var h48={a:"库房和食品贮存场所（11分）",i:"48",b:"冷藏、冷冻柜（库）数量和结构能使原料、半成品和成品分开存放，有明显区分标识。",c:"※"};
	var h49={a:"库房和食品贮存场所（11分）",i:"49",b:"除冷库外的库房有良好的通风、防潮、防鼠（如设防鼠板或木质门下方以金属包覆）设施。",c:"2"};
	var h50={a:"库房和食品贮存场所（11分）",i:"50",b:"冷藏、冷冻库设可正确指示库内温度的温度计。",c:"2"};
	var h51={a:"库房和食品贮存场所（11分）",i:"51",b:"库房及冷藏、冷冻库内应设置数量足够的物品存放架，能使贮存的食品离地离墙存放。",c:"2"};
	
	var h52={p:5,a:"专间要求（10分）",i:"52",b:"专间内无明沟，地漏带水封，专间墙裙铺设到顶。",c:"5"};
	var h53={a:"专间要求（10分）",i:"53",b:"专间只设一扇门，采用易清洗、不吸水的坚固材质，能够自动关闭。窗户封闭。",c:"5"};
	var h54={a:"专间要求（10分）",i:"54",b:"需要直接接触成品的用水，应加装水净化设施。",c:"※"};
	var h55={a:"专间要求（10分）",i:"55",b:"专间内设符合餐饮服务食品安全管理规范要求的空调设施、空气消毒设施、流动水源、工具清洗消毒设施；凉菜间、食品冷却间、食品包装间设专用冷冻（藏）设施。",c:"※"};
	var h56={a:"专间要求（10分）",i:"56",b:"专间入口处设置有非手动式洗手、消毒、更衣设施的通过式预进间。",c:"※"};
	
	var h57={p:1,a:"更衣室（2分）",i:"57",b:"更衣场所应与加工经营场所处于同一建筑物内，有足够大小的空间、足够数量的更衣设施和适当的照明。",c:"2"};
	
	var h58={p:3,a:"厕所（7分）",i:"58",b:"厕所不设在食品处理区。",c:"※"};
	var h59={a:"厕所（7分）",i:"59",b:"厕所采用水冲式，在出口附近设置洗手设施。",c:"2"};
	var h60={a:"厕所（7分）",i:"60",b:"厕所排污管道与食品加工操作场所的排水管道分设，并有可靠的防臭气水封。",c:"5"};
	
	var h61={p:1,a:"运输设备",i:"61",b:"配备与加工食品品种、数量以及贮存要求相适应的封闭式专用运输冷藏车辆，车辆内部结构平整，易清洗",c:"※"};
	
	var h62={p:3,a:"食品检验和留样（10分）",i:"62",b:"设置与加工制作的食品品种相适应的检验室",c:"5"};
	var h63={a:"食品检验和留样（10分）",i:"63",b:"配备与检验项目相适应的检验设施和检验人员。",c:"5"};
	var h64={a:"食品检验和留样（10分）",i:"64",b:"配备留样专用容器和冷藏设施，以及留样管理人员。。",c:"※"};
	
	
	//附件2.国境口岸航空配餐企业卫生许可现场审查表
	var o1={p:4,a:"选址(15分)",i:"1",b:"不应选择对食品有显著污染的区域，以及有害废弃物、粉尘、有害气体、放射性物质和其他扩散性污染源不能有效清除的地址。",c:"※"};
	var o2={a:"选址(15分)",i:"2",b:"周围不宜有虫害大量孳生的潜在场所，难以避开时应设计必要的防范措施。",c:"10"};
	var o3={a:"选址(15分)",i:"3",b:"水源供应充足，水质符合《生活饮用水卫生标准》等要求。",c:"※"};
	var o4={a:"选址(15分)",i:"4",b:"地势平坦，干燥，易于排水。",c:"5"};
	
	var o5={p:14,a:"厂房和车间(52分)",i:"5",b:"厂区布局合理，环境整洁，厂区道路硬化，划分生产区和生活区，生产区应在生活区的上风向。",c:"※"};
	var o6={a:"厂房和车间(52分)",i:"6",b:"厂房的面积和空间应与生产能力相适应，便于设备安置、清洁消毒、物料储存及人员操作。",c:"※"};
	var o7={a:"厂房和车间(52分)",i:"7",b:"食品生产区应设置专用的初加工、冷加工、热加工、分装、装配、储存及餐用具清洗消毒场所。冷食加工及分装、热食加工及分装应分别设置相应专间，洁净餐具存放应设置相应专区。",c:"10"};
	var o8={a:"厂房和车间(52分)",i:"8",b:"厂房和车间的设计与布局应能满足生产工艺流程和卫生操作需要， 按原料验收-储存-初加工-冷、热加工-分装-装配-成品储存-配送装机的生产流程合理布局，避免食品交叉污染。",c:"※"};
	var o9={a:"厂房和车间(52分)",i:"9",b:"厂房和车间应合理划分作业区，并采取有效分离或分隔。",c:"5"};
	var o10={a:"厂房和车间(52分)",i:"10",b:"厂房内设置的检验室应与生产区域分隔。",c:"2"};
	var o11={a:"厂房和车间(52分)",i:"11",b:"食品原料入口及通道、餐食配送通道及出口、垃圾通道及出口.飞机餐食/餐具回收入口通道、人员入口应当分开设置。",c:"5"};
	var o12={a:"厂房和车间(52分)",i:"12",b:"建筑内部结构应易于维护、清洁或消毒。应采用适当的耐用材料建造。",c:"※"};
	var o13={a:"厂房和车间(52分)",i:"13",b:"车间地面应使用无毒、无味、不渗透、耐腐蚀的材料建造，结构应有利于排污和清洗的需要。",c:"5"};
	var o14={a:"厂房和车间(52分)",i:"14",b:"地面应平坦防滑、无裂缝、并易于清洁、消毒，并有适当的措施防止积水。",c:"5"};
	var o15={a:"厂房和车间(52分)",i:"15",b:"墙壁、隔断应使用无毒、无味、的防渗透材料建造，在操作高度范围内的墙面应光滑、不易积累污垢且易于清洁。",c:"5"};
	var o16={a:"厂房和车间(52分)",i:"16",b:"墙壁、隔断和地面交界处应结构合理、易于清洁，能有效避免污垢积存。",c:"5"};
	var o17={a:"厂房和车间(52分)",i:"17",b:"顶棚应使用无毒、无味、与生产需求相适应、易于观察清洁状况的材料建造，表面涂层不易脱落，在结构上不利于冷凝水垂直滴下。",c:"5"};
	var o18={a:"厂房和车间(52分)",i:"18",b:"门、窗严密，有防蝇、防尘、防鼠设施，防护门能自动闭合，位置合理，窗户如设置窗台，其结构应能避免灰尘积存且易于清洁。",c:"5"};
	
	var o19={p:33,a:"设施与设备卫生要求(124分)",i:"19",b:"食品加工用水的水质应符合GB5749的规定。",c:"※"};
	var o20={a:"设施与设备卫生要求(124分)",i:"20",b:"食品加工用水与其他不与食品接触的用水（如间接冷却水、污水或废水等）应以完全分离的管路输送，避免交叉污染。各管路系统应明确标识以便区分。",c:"10"};
	var o21={a:"设施与设备卫生要求(124分)",i:"21",b:"排水系统地设计和建造应保证排水通畅、便于清洁维护；应适应食品生产的需要，保证食品及生产、清洁用水不受污染。",c:"※"};
	var o22={a:"设施与设备卫生要求(124分)",i:"22",b:"排水系统入口应安装带水封的地漏等装置，以防止固体废弃物进入及浊气溢出，出口处应有适当措施以降低虫害风险。",c:"5"};
	var o23={a:"设施与设备卫生要求(124分)",i:"23",b:"室内排水的流向应由清洁程度要求高的区域流向清洁程度要求低的区域，且有防止逆流的设计。",c:"5"};
	var o24={a:"设施与设备卫生要求(124分)",i:"24",b:"污水在排放前应经适当的方式处理，以符合国家污水排放的相关规定。",c:"2"};
	var o25={a:"设施与设备卫生要求(124分)",i:"25",b:"应配备足够的食品、工器具和设备的专用清洁设施和消毒设施，并单独存放，其位置不会污染食品及其加工操作过程。",c:"※"};
	var o26={a:"设施与设备卫生要求(124分)",i:"26",b:"各场所设置密闭的废弃物盛放容器（堆放加工边角料容器除外），容器应标识清洗，专间内的废弃物容器盖子为非手动开启式。。",c:"5"};
	var o27={a:"设施与设备卫生要求(124分)",i:"27",b:"生产场所入口处应设置更衣室；必要时特定的作业区（比如专间）入口处可按需要设置更衣室。更衣室应保证工作服与私人物品个人服装及其他物品分开放置。",c:"※"};
	var o28={a:"设施与设备卫生要求(124分)",i:"28",b:"生产场所入口处应设置换鞋（穿戴鞋套）设施或工作鞋靴消毒设施。",c:"5"};

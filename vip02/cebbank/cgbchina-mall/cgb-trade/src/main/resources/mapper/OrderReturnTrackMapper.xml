<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="OrderReturnTrack">
    <resultMap id="OrderReturnTrackMap" type="OrderReturnTrackModel">
        <result property="partbackId" column="partback_id"/>
        <result property="id" column="id"/>
        <result property="status" column="status"/>
        <result property="createTime" column="create_time"/>
        <result property="createOper" column="create_oper"/>
        <result property="modifyOper" column="modify_oper"/>
        <result property="modifyTime" column="modify_time"/>
        <result property="delFlag" column="del_flag"/>
    </resultMap>
    <sql id="columns">
        partback_id,id,status,create_time,create_oper,modify_oper,modify_time,del_flag
    </sql>

    <insert id="insert" parameterType="OrderReturnTrackModel" useGeneratedKeys="true"
            keyProperty="id" keyColumn="id">
        insert into tbl_order_return_track
        (partback_id
        ,status
        ,create_time
        ,create_oper
        ,modify_oper
        ,modify_time
        ,del_flag)
        values(#{partbackId}
        ,#{status}
        ,now()
        ,#{createOper}
        ,#{modifyOper}
        ,#{modifyTime}
        ,#{delFlag})
    </insert>

    <update id="update" parameterType="OrderReturnTrackModel">
        update tbl_order_return_track set
        <if test="partbackId !=null">partback_id = #{partbackId}
            ,
        </if>
        <if test="status !=null">status = #{status}
            ,
        </if>
        <if test="createTime !=null">create_time = #{createTime}
            ,
        </if>
        <if test="createOper !=null">create_oper = #{createOper}
            ,
        </if>
        <if test="modifyOper !=null">modify_oper = #{modifyOper}
            ,
        </if>
        <if test="modifyTime !=null">modify_time = #{modifyTime}
            ,
        </if>
        <if test="delFlag !=null">del_flag = #{delFlag}
        </if>
        where id = #{id}
    </update>

    <sql id="conditions">
        <where>
            <if test="partbackId !=null">and partback_id = #{partbackId}</if>
            <if test="id !=null">and id = #{id}</if>
            <if test="status !=null">and status = #{status}</if>
            <if test="createTime !=null">and create_time = #{createTime}</if>
            <if test="createOper !=null">and create_oper = #{createOper}</if>
            <if test="modifyOper !=null">and modify_oper = #{modifyOper}</if>
            <if test="modifyTime !=null">and modify_time = #{modifyTime}</if>
            <if test="delFlag !=null">and del_flag = #{delFlag}</if>
        </where>
    </sql>
    <select id="findById" parameterType="Map" resultMap="OrderReturnTrackMap">
        SELECT id,
        <include refid="columns"/>
        from tbl_order_return_track
        where id=#{id}
    </select>


    <select id="findByPartbackId" parameterType="Map" resultMap="OrderReturnTrackMap">
        SELECT partback_id,
        <include refid="columns"/>
        from tbl_order_return_track
        where partback_id=#{partbackId}
        <![CDATA[ order by create_time desc ]]>

    </select>


    <select id="findAll" resultMap="OrderReturnTrackMap">
        select id,
        <include refid="columns"/>
        from tbl_order_return_track
        <include refid="conditions"/>
    </select>

    <select id="pager" parameterType="Map" resultMap="OrderReturnTrackMap">
        select id,
        <include refid="columns"/>
        from tbl_order_return_track
        <include refid="conditions"/>
        <![CDATA[ order by id desc limit #{offset}, #{limit} ]]>
    </select>
    <delete id="delete" parameterType="OrderReturnTrackModel">
        delete from tbl_order_return_track where id = id
    </delete>


    <insert id="insertList" parameterType="java.util.List" useGeneratedKeys="true"
            keyProperty="id" keyColumn="id">
        insert into tbl_order_return_track
        (partback_id
        ,status
        ,create_time
        ,create_oper
        ,modify_oper
        ,modify_time
        ,del_flag)
        values
        <foreach collection="orderReturnTrackModelList" item="item" index="index" separator=",">
            (#{item.partbackId},#{item.status},#{item.now()},#{item.createOper},,#{item.modifyOper},#{item.modifyTime}#{item.delFlag})
        </foreach>
    </insert>
</mapper>


<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet idx="0" >
    	<!--保留空白位置，保证loop开始读取位置正确-->
    	<section startRow="0" endRow="2">
    		
    	</section>
    	<!-- items:对应Java里面的Map<> bean的key -->
        <loop startRow="3" endRow="3" items="students" var="student" varType="com.my.test.Student">
            <section startRow="3" endRow="3">
                <mapping row="3" col="0">student.name</mapping>
                <mapping row="3" col="1">student.age</mapping>
            </section>
            <!--循环结束条件 最后离结束内容的偏移 end 为Sample中，列表最后一行的内容 可空白-->
            <loopbreakcondition>
            		<!--offset 行偏移 正数n为读取当前行时先判断往下n行是否符合结束条件 负数测试过，走不通读取数据-->
                    <rowcheck offset="0">
                    	<!--offset 为判断该行那一列(个)单元格内容-->
                        <cellcheck offset="0">end</cellcheck>
                    </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>
        </if>
        <if test="modifyTime !=null">modify_time = #{modifyTime}
            ,
        </if>
        <if test="reserved1 !=null">reserved1 = #{reserved1}
        </if>
        where id = #{id}
    </update>

    <sql id="conditions">
        <where>
            <if test="id !=null">and id = #{id}</if>
            <if test="vendorInfId !=null">and vendor_inf_id = #{vendorInfId}</if>
            <if test="transcorpName !=null">and transcorp_name = #{transcorpName}</if>
            <if test="contact !=null">and contact = #{contact}</if>
            <if test="mobile !=null">and mobile = #{mobile}</if>
            <if test="phone !=null">and phone = #{phone}</if>
            <if test="fax !=null">and fax = #{fax}</if>
            <if test="address !=null">and address = #{address}</if>
            <if test="email !=null">and email = #{email}</if>
            <if test="servicePhone !=null">and service_phone = #{servicePhone}</if>
            <if test="serviceUrl !=null">and service_url = #{serviceUrl}</if>
            <if test="transCorpDesc !=null">and trans_corp_desc = #{transCorpDesc}</if>

    <sql id="columns">
        id,vendor_inf_id,transcorp_name,contact,mobile,phone,fax,address,email,service_phone,service_url,trans_corp_desc,status,create_oper,modify_oper,create_time,modify_time,reserved1
    </sql>

    <insert id="insert" parameterType="VendorTranscorpModel" useGeneratedKeys="true"
            keyProperty="id" keyColumn="id">
        insert into tbl_vendor_transcorp
        (vendor_inf_id
        ,transcorp_name
        ,contact
        ,mobile
        ,phone
        ,fax
        ,address
        ,email
        ,service_phone
        ,service_url
        ,trans_corp_desc
        ,status
        ,create_oper
        ,modify_oper
        ,create_time
        ,modify_time

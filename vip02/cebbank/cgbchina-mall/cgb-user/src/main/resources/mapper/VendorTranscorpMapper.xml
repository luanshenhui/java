        ,#{transcorpName}
        ,#{contact}
        ,#{mobile}
        ,#{phone}
        ,#{fax}
        ,#{address}
        ,#{email}
        ,#{servicePhone}
        ,#{serviceUrl}
        ,#{transCorpDesc}
        ,#{status}
        ,#{createOper}
        ,#{modifyOper}
        ,#{createTime}
        ,#{modifyTime}
        ,#{reserved1})
    </insert>

    <update id="update" parameterType="VendorTranscorpModel">
        update tbl_vendor_transcorp set
        <if test="vendorInfId !=null">vendor_inf_id = #{vendorInfId}
            ,
        </if>
        <if test="transcorpName !=null">transcorp_name = #{transcorpName}
            ,
        </if>
        <if test="contact !=null">contact = #{contact}
            ,
        </if>
        <if test="mobile !=null">mobile = #{mobile}
            ,
        </if>
        <if test="phone !=null">phone = #{phone}
            ,
        </if>
        <if test="fax !=null">fax = #{fax}
            ,
        </if>
        <if test="address !=null">address = #{address}
            ,
        </if>
        <if test="email !=null">email = #{email}
            ,
        </if>
        <if test="servicePhone !=null">service_phone = #{servicePhone}
            ,
        </if>
        <if test="serviceUrl !=null">service_url = #{serviceUrl}
            ,
        </if>
        <if test="transCorpDesc !=null">trans_corp_desc = #{transCorpDesc}
            ,
        </if>
        <if test="status !=null">status = #{status}
            ,
        </if>
        <if test="createOper !=null">create_oper = #{createOper}
            ,
        </if>
        <if test="modifyOper !=null">modify_oper = #{modifyOper}
            ,
        </if>
        <if test="createTime !=null">create_time = #{createTime}

        </where>
    </sql>
    <select id="findById" parameterType="Map" resultMap="VendorTranscorpMap">
        SELECT id,
        <include refid="columns"/>
        from tbl_vendor_transcorp
        where id=#{id}
    </select>
    <select id="findAll" resultMap="VendorTranscorpMap">
        select id,
        <include refid="columns"/>
        from tbl_vendor_transcorp
        <include refid="conditions"/>
    </select>

    <select id="pager" parameterType="Map" resultMap="VendorTranscorpMap">
        select id,
        <include refid="columns"/>
        from tbl_vendor_transcorp
        <include refid="conditions"/>
        <![CDATA[ order by id desc limit #{offset}, #{limit} ]]>
    </select>
    <delete id="delete" parameterType="VendorTranscorpModel">
        delete from tbl_vendor_transcorp where id = id
    </delete>
</mapper>



<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ToolsGoods">
    
    <insert id="insertCategoryRelation" parameterType="cn.com.cgbchina.batch.tools.Transfer" useGeneratedKeys="true"
            keyProperty="id" keyColumn="id">
        insert into tbl_category_relation
        (categoryid,categoryname,parentid,childrenids,attributesKeys,attributesValues,spus)
        values
        (#{categoryid},#{categoryname},#{parientid},#{childrenids},#{attributesKeys},#{attributesValues},#{spus})
    </insert>
	
    <delete id="deleteCategoryRelation" >
        delete from tbl_category_relation
    </delete>
	
	
	
</mapper>



<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet idx="0" >
    	<!--保留空白位置，保证loop开始读取位置正确-->
    	<section startRow="0" endRow="1">
    		
    	</section>
    	<!-- items:对应Java里面的Map<> bean的key -->
        <loop startRow="2" endRow="2" items="items" var="item" varType="cn.com.cgbchina.item.dto.GoodsImportDto">
            <section startRow="2" endRow="2">
				<mapping row="2" col="0">item.successFlag</mapping>
				<mapping row="2" col="1">item.failReason</mapping>
                <mapping row="2" col="2">item.goodsCode</mapping>
				<mapping row="2" col="3">item.goodsName</mapping>
				<mapping row="2" col="4">item.productName</mapping>
				<mapping row="2" col="5">item.manufacturer</mapping>
				<mapping row="2" col="6">item.vendorId</mapping>
				<mapping row="2" col="7">item.brandName</mapping>
				<mapping row="2" col="8">item.backCategory1Id</mapping>
				<mapping row="2" col="9">item.backCategory2Id</mapping>
				<mapping row="2" col="10">item.backCategory3Id</mapping>
				<mapping row="2" col="11">item.goodsType</mapping>
				<mapping row="2" col="12">item.isInner</mapping>
				<mapping row="2" col="13">item.cards</mapping>
				<mapping row="2" col="14">item.mailOrderCode</mapping>
            </section>
            <!--循环结束条件 最后离结束内容的偏移 end 为Sample中，列表最后一行的内容 可空白-->
            <loopbreakcondition>
            		<!--offset 行偏移 正数n为读取当前行时先判断往下n行是否符合结束条件 负数测试过，走不通读取数据-->
                    <rowcheck offset="0">
                    	<!--offset 为判断该行那一列(个)单元格内容-->
						<cellcheck offset="3"></cellcheck>
						<cellcheck offset="6"></cellcheck>
                    </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>
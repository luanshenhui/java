        {{#equals "1" vendorModel.checkStatus }}
            <input name="vendorModel.checkStatus" value="2" type="hidden" class="form-control"/>
        {{else}}
            {{#equals "0" vendorModel.checkStatus}}
                <input name="vendorModel.checkStatus" value="0" type="hidden" class="form-control"/>
            {{else}}
                {{#equals "2" vendorModel.checkStatus }}
                    <input name="vendorModel.checkStatus" value="2" type="hidden" class="form-control"/>
                {{else}}
                    {{#equals "3" vendorModel.checkStatus}}
                        <input name="vendorModel.checkStatus" value="0" type="hidden" class="form-control"/>
                    {{else}}
                        {{#equals "4" vendorModel.checkStatus}}
                            <input name="vendorModel.checkStatus" value="2" type="hidden" class="form-control"/>

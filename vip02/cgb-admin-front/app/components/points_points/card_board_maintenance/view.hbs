{{#component "card-board-maintenance js-component"}}
    <p class="components-title"><strong>卡类卡板维护</strong></p>
    <div class="add-new">
        <button class="btn btn-success btn-sm js-card-new m-b-10" >新增</button>
    </div>

    <table class="table">
        <thead>
        <tr>
            <th width="5%">选择</th>
            <th width="10%">卡产品名称</th>
            <th width="5%">卡等级</th>
            <th width="5%">卡等级编码</th>
            <th width="5%">卡bin</th>
            <th width="5%">分行</th>
            <th width="5%">分行号</th>
            <th width="10%">联名卡代号</th>
            <th width="5%">第三等级卡产品编码</th>
            <th width="5%">卡类</th>
            <th width="5%">推广计划1</th>
            <th width="5%">推广计划2</th>
            <th width="5%">推广计划3</th>
            <th width="5%">推广计划4</th>
            <th width="5%">积分类型</th>
            <th width="15%">操作</th>
        </tr>
        </thead>
        <tbody>
        {{#each _DATA_.data}}
        <tr data-data="{{json this}}">
            <td><input type="checkbox" name="checkCard"></td>
            <td class="card-name">{{cardName}}</td>
            <td class="card-level">{{cardLevel}}</td>
            <td class="card-code">{{cardLevelCode}}</td>
            <td class="card-bin">{{cardBin}}</td>
            <td class="card-branch">{{branch}}</td>
            <td class="card-branch-code">{{BranchCode}}</td>
            <td class="card-joint-code">{{jointCardCode}}</td>
            <td class="card-third-level">{{thirdLevelCode}}</td>
            <td class="card-type">{{cardType}}</td>
            <td class="card-promotion-plan-one">{{promotionPlanOne}}</td>
            <td class="card-promotion-plan-two">{{promotionPlanTwo}}</td>
            <td class="card-promotion-plan-three">{{promotionPlanThree}}</td>
            <td class="card-promotion-plan-four">{{promotionPlanFour}}</td>
            <td class="card-points-type">{{pointsType}}</td>
            <td>
                <button class="btn btn-sm js-card-edit">编辑</button>
                <button id="{{id}}" class="btn btn-sm js-card-delete" data-toggle="confirm"
                        data-title="您确定删除吗?" data-content="删除之后无法恢复,确定要删除吗？"
                        data-event="confirm:delete-card">删除</button>
            </td>
        </tr>
        {{/each}}
        </tbody>
    </table>
    <div class="m-b-10 m-l-10">
        <input type="checkbox" class="js-all-check"/><span>全选</span>
        <a href="javascript:void(0)" class="js-card-delete">删除</a>
    </div>
    <div class="pagination" data-total="{{_DATA_.total}}"></div>
{{/component}}
  font-size: 12px !important;
}

.bg_red {
  background: red none repeat scroll 0 0;
  color: #fff;
  display: inline-block;
  line-height: 20px;
  text-align: center;
  width: 20px;
}

.bg_yellow {
  background: #fc0 none repeat scroll 0 0;
  color: #fff;
  display: inline-block;
  line-height: 20px;
  text-align: center;
  width: 20px;
}

.bg_green {
  background: #390 none repeat scroll 0 0;
  color: #fff;
  display: inline-block;
  line-height: 20px;
  text-align: center;
  width: 20px;
}

.btn-import {
  border: medium none;
  cursor: pointer;
  display: inline-block;
  font-size: 14px;
  font-weight: normal;
  line-height: 1.42857;
  margin-bottom: 0;
  margin-left: 10px;
  padding: 5px 12px;
  text-align: center;
  vertical-align: middle;
  white-space: nowrap;
  background-color: #f01919;
  border: 2px solid #f01919;
  color: #fff;
  -moz-border-radius: 5px; /* Firefox */
  -webkit-border-radius: 5px; /* Safari 和 Chrome */
  border-radius: 5px; /* Opera 10.5+, 以及使用了IE-CSS3的IE浏览器 */
}

.btn-upload {
  display: inline-block;
  line-height: normal !important;
  margin: 0 auto;
  padding: 5px 20px;
  -webkit-border-radius:4px;
  -moz-border-radius:4px;
  border-radius:4px;
  text-align: center;
  text-decoration: none;
  background-color: #f01919;
  color: #fff;
}

.ztree-box {
  display: inline-block;
}

.clear {
  font-size: 0px;
  clear: both;
  line-height: 0px;
  height: 1px;
}

.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

.clearfix {
  min-height: 1px;
}

* html .clearfix {
  height: 1px;
}

img {
  border: none;
}

.text-overflow {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.filter-name {
  margin: 0 0 5px 0;
}

.form-aligned {
  .control-group {
    margin-bottom: 10px !important;
  }
}

.remaining-text {
  text-align: right;
  margin: 0;
  color: #ccc;
}
.body-mode{
  display:none;
  position: fixed;
  width: 100%;
  height: 100%;
  background: #fff;
  z-index: 999999999;
  opacity: 0.3;
  //filter:alpha(opacity=30);
  -moz-opacity:0.3;
  -khtml-opacity: 0.3;

}

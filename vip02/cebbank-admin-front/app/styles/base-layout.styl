@charset "UTF-8";
html {
  margin: 0;
  _padding: 60px 0 40px 0;
  _border: 0;
}

*::after, *::before {
  box-sizing: border-box;
}

body {
  font-size: 90%;
  color: #333;
  font-family: "Microsoft YaHei", "微软雅黑", "Microsoft Yahei", "simsun";
}

ul {
  margin: 0px;
  padding: 0px;
  li {
    margin: 0px;
    padding: 0px;
  }
}

.outer {
  width: 100%;
  position: absolute;
  top: 61px;
  bottom: 40px;
  overflow: hidden !important;
  _position: relative; /**IE6***/
  _top: 0px;
  _OVERFLOW-Y: auto;
  _OVERFLOW-X: auto;
  *height: 100%;
}

a:link {
  TEXT-DECORATION: none;
  outline: none;
}

a:visited {
  TEXT-DECORATION: none;
  outline: none;
}

a:hover {
  TEXT-DECORATION: none;
  outline: none;
}

a:active {
  TEXT-DECORATION: none;
  outline: none;
}

.btn {
  &.btn-info {
    &:hover {
      background-color: #f01919;
      color: #FFFFFF;
    }
  }

}
.main {
  overflow: hidden;
  margin: 0 auto;
  height: 100%;
  width: auto !important;
  .category-list {
    vertical-align: middle;
    height: 100%;
    .category-container {
      float: left;
      vertical-align: middle;
      height: 100%;
    }
  }
}

.sidebar-outer {
  float: left;
  background-color: #F4F4F4;
  overflow: hidden;
}

.body-outer {
  float: left;
  width: 1060px;
  padding-bottom: 50px;
  margin-right: 10px;
}

.front-main {
  background-color: #FFFFFF;
}

.content {
  background-color: #FFFFFF;
  min-height: 600px;
}

.content-box {
  width: 1180px;
  margin: 0 auto;
}

.get-properties {
  padding: 0 1em;
  ul {
    list-style: none;
  }
}

.edit-area {
  height: 100%;
  float: right;
  .back-area {
    height: 100%;
  }
  .front-area {
    height: 100%;
  }
}

.footer {
  border-top: 1px solid #eeeeee;
  position: absolute;
  bottom: 0;
  left: 0;
  height: 15px;
  width: 100%;
  text-align: center;
  padding: 15px 0 10px 0;
  overflow: hidden !important; /*FF*/
  _OVERFLOW-Y: auto; /*IE6*/
  _OVERFLOW-X: auto;

}

.copyright {
  width: 100%;
}

.front-footer {
  border-top: 1px solid #ccc;
  height: 35px;
  line-height: 35px;
  width: 100%;
  text-align: center;
  padding: 15px 0 10px 0;
  background: #fff;
}

.span-error {
  color: #c00031 !important;
}

.text-left {
  text-align: left !important;
}

.container-components {
  float: right;
  width: 960px;
  padding-bottom: 50px;
  table {
    width: 100%;
    table-layout: fixed;
    /*margin-bottom: 10px;*/
  }
}

.span4 {
  padding: 0 5px;
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: middle;
  zoom: 1;
  width: 300px;
  text-align: right;
}

.parent-position {
  position: relative;
}

.components-title {
  font-size: 14px;
  color: #333;
  span {
    font-size: 12px;
    color: #cccccc;
  }

}

.btn-group {
  vertical-align: middle;
  margin-bottom: 5px;
  ul {
    background-color: #fff;
    list-style-type: none !important;
    li {
      background-color: #fff;
      border: none !important;
    }
  }
}

.font-color-red {
  color: #ed1a2e !important;
}

.font-color-green {

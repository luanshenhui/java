<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
        <struts>
    	<!-- 各个功能模块的跳转 -->
    	<package name="change" namespace="/change" extends="netctoss">
    		<action name="toIndex">
    			<interceptor-ref name="loginStack"/>
    			<result>/WEB-INF/NETCTOSS/main/index.jsp</result>
    		</action>
    		<action name="toModiPwd">
    			<interceptor-ref name="loginStack"/>
    			<result>/WEB-INF/NETCTOSS/user/user_modi_pwd.jsp</result>
    		</action>
    	</package>
    </struts>
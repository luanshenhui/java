package User;

import java.util.Scanner;

public class Sys {
	User[]userList;
	
	private UserService us;
	
	private User u;//保存已经登陆的用户，登陆成功后给她赋值
	public Sys(){
		userList=new User[0];
		us=new UserService();
	}
//注册，登陆，查看
	public void regist(){
		Scanner sc=new Scanner(System.in);
		System.out.print("请输入你的用户名");
		String name =sc.next();
		System.out.print("请输入你的密码");
		String pwd =sc.next();
		User u=new User();
		u.setUserName(name);
		u.setPassword(pwd);
		us.regist(u, this);//this指代当前对象
		
	
		
	}
	public User[] getUserList() {
		return userList;
	}
	public void setUserList(User[] userList) {
		this.userList = userList;
	}
	public UserService getUs() {
		return us;
	}
	public void setUs(UserService us) {
		this.us = us;
	}
	public User getU() {
		return u;
	}
	public void setU(User u) {
		this.u = u;
	}
	public void longin(){
		
	}
	public void look(){
		
	}
	public User[] getUserlist() {
		// TODO Auto-generated method stub
		return null;
	}
}

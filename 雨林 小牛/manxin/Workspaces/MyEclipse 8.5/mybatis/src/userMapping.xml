<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userMapping">
	<select id="findUserById" parameterType="int" resultType="com.yulin.User.User">
		select * from t_user where t_id = #{id};
	</select>
	<insert id="insertUser" parameterType="com.yulin.User.User">
		insert into t_user values(#{t_id},#{t_name},#{t_age});
	</insert>
</mapper>
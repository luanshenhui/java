package com.yulin.web.service;

import java.util.List;

import com.yulin.web.dao.UserDaoImpl;
import com.yulin.web.entity.User;

public class UserService {
	public List<User> findAll(){
		return new UserDaoImpl().findAll();
	}
	
	public boolean insertUser(User u){
		return new UserDaoImpl().insertUser(u);
	}
	
	public boolean updateUser(User u){
		return new UserDaoImpl().updateUser(u);
	}
	
	public int findMinSalary(int salary){
		return new UserDaoImpl().findMinSalary(salary);
	}
	
	public int findMaxSalary(int salary){
		return new UserDaoImpl().findMaxSalary(salary);
	}
}

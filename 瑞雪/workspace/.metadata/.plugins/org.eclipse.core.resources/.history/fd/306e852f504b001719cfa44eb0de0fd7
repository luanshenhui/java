package cn.rkylin.oms.system.logistics.dao;

import java.util.List;

import cn.rkylin.oms.system.logistics.domain.OMS_LGST;
import cn.rkylin.oms.system.logistics.vo.LogisticsVO;

public interface ILogisticsDAO {

	List getLgstByCondition(OMS_LGST unitParam, int page, int rows) throws Exception;
	
	void delete(String stationParam,String string)throws Exception;
	
	List getLgstByCondition(LogisticsVO lgstVO) throws Exception;
	
	public void insert(LogisticsVO lgstVO) throws Exception;
	
	void update(OMS_LGST ur)throws Exception;
	
	OMS_LGST findByid(String lgstId) throws Exception; 
}

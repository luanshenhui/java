<body>  
<div class="page-content">
	<div class="container-fluid">
		<div class="row">
						<div class="col-lg-4">
							<div class="portlet box portlet-violet">
								<div class="portlet-header">
									<div class="caption">功能菜单</div>
								</div>
								<div class="portlet-body">
									<div id="menuTree" style="height:380px;overflow:auto;"></div>
								</div>
							</div>
						</div>
						
						
						<div class="col-lg-8">
							<div class="portlet box portlet-blue tabbable">
								<div class="portlet-header">
									<div class="caption">菜单详细信息</div>
								</div>
								<div class="portlet-body">
									 	<form id="menuDetail"  action="" class="form-horizontal">
                					
		                					<div class="form-group">
												<label for="menuName" class="col-sm-3 control-label">名称:<span class='require'>*</span>
												</label>
												<div class="col-sm-9">
				                                    <input id="menuName" name="menuName" type="text" placeholder="" required maxlength="32" class="form-control  invalid " />
												</div>
											</div>
		                					
		                					<div class="form-group">
												<label for="menuImg" class="col-sm-3 control-label">图片路径:<span class='require'></span>
												</label>
												<div class="col-sm-9">
				                                     <input id="menuImg" name="menuImg" type="text" placeholder=""  maxlength="255" class="form-control " />
												</div>
											</div>
											
											<div class="form-group">
												<label for="menuDes" class="col-sm-3 control-label">描述:<span class='require'></span>
												</label>
												<div class="col-sm-9">
				                                     <input id="menuDes" name="menuDes" type="text" placeholder=""  maxlength="255" class="form-control " />
												</div>
											</div>
											
											<div class="form-group">
												<label for="menuLocal" class="col-sm-3 control-label">请求URL:<span class='require'></span>
												</label>
												<div class="col-sm-9">
				                                     <input id="menuLocal" name="menuLocal" type="text" placeholder=""  maxlength="255" class="form-control " />
												</div>
											</div>
		                					
		                					<div class="form-group">
												<label for="menuDis" class="col-sm-3 control-label">默认显示
												</label>
												<div class="col-sm-7">
				                                     <select  id="menuDis" name="menuDis" >
																<option value="0">否</option>
																<option value="1">是</option>
													</select>
				                                </div>
											</div>
											
			                				<div class="modal-footer">
						                   	 	<button type="button"  class="btn btn-primary" id="btn_menuReset" onClick="menuReset();">重置</button>
						                        <button id="btn_menuSave" type="button" class="btn btn-primary" onclick="saveMenu()">保存</button>
						                    </div>
		                				</form>
		                				
		                				<form id="elementDetail"  action="" class="form-horizontal">
                					
		                					<div class="form-group">
												<label for="eleId" class="col-sm-3 control-label">元素ID:<span class='require'>*</span>
												</label>
												<div class="col-sm-9">
				                                    <input id="eleId" name="eleId" type="text" placeholder="" required maxlength="32" class="form-control  invalid " />
				                                    
												</div>
											</div>
		                					
		                					<div class="form-group">
												<label for="eleName" class="col-sm-3 control-label">元素名称:<span class='require'>*</span>
												</label>
												<div class="col-sm-9">
				                                     <input id="eleName" name="eleName" required type="text" placeholder=""  maxlength="255" class="form-control " />
												</div>
											</div>
											
											<div class="form-group">
												<label for="eleImg" class="col-sm-3 control-label">图片路径:<span class='require'></span>
												</label>
												<div class="col-sm-9">
				                                     <input id="eleImg" name="eleImg" type="text" placeholder=""  maxlength="255" class="form-control " />
												</div>
											</div>
											
											<div class="form-group">
												<label for="menuLocal" class="col-sm-3 control-label">元素类型:<span class='require'>*</span>
												</label>
												<div class="col-sm-9">
												 		<select id="eleType" type="eleType" placeholder="" required maxlength="255" class="form-control "></select>
				                                   <!--  <input id="eleType" name="eleType" type="text" placeholder=""  maxlength="255" class="form-control " />-->
												</div>
											</div>
											
											<div class="form-group">
												<label for="eleDes" class="col-sm-3 control-label">描述:<span class='require'></span>
												</label>
												<div class="col-sm-9">
				                                     <input id="eleDes" name="eleDes" type="text" placeholder=""  maxlength="255" class="form-control " />
												</div>
											</div>
		                					
		                					
		                					
		               					<div class="modal-footer">
					                   	 	<button type="button"  class="btn btn-primary" id="btn_eleReset" onClick="eleReset();">重置</button>
					                        <button id="btn_eleSave" type="button" class="btn btn-primary" onclick="saveEle()">保存</button>
					                    </div>
		                		
		                				</form>
		                				
		                				
		                			<form id="pageDetail"   action="" class="form-horizontal">
                					
                					<div class="form-group">
										<label for="menuName" class="col-sm-3 control-label">页面ID:<span class='require'>*</span>
										</label>
										<div class="col-sm-9">
		                                    <input id="pageId" name="pageId" type="text"  placeholder=""  required  maxlength="32"  class="form-control  invalid " />
		                                    
										</div>
									</div>
                					
                					<div class="form-group">
										<label for="pageName" class="col-sm-3 control-label">页面名称:<span class='require'></span>
										</label>
										<div class="col-sm-9">
		                                     <input id="pageName" name="pageName" type="text" placeholder=""  maxlength="255" class="form-control " />
										</div>
									</div>
									
									<div class="form-group">
										<label for="pageImg" class="col-sm-3 control-label">图片路径:<span class='require'></span>
										</label>
										<div class="col-sm-9">
		                                     <input id="pageImg" name="pageImg" type="text" placeholder=""  maxlength="255" class="form-control " />
										</div>
									</div>
									
									<div class="form-group">
										<label for="pageDes" class="col-sm-3 control-label">描述:<span class='require'></span>
										</label>
										<div class="col-sm-9">
		                                     <input id="pageDes" name="pageDes" type="text" placeholder=""  maxlength="255" class="form-control " />
										</div>
									</div>
									
								
                					<div class="form-group">
										<label for="pageLocal" class="col-sm-3 control-label">请求URL:<span class='require'></span>
										</label>
										<div class="col-sm-9">
		                                     <input id="pageLocal" name="pageLocal" type="text" placeholder=""  maxlength="255" class="form-control " />
										</div>
									</div>
                					
                					<div class="form-group">
										<label for="pageDis" class="col-sm-3 control-label">默认显示
										</label>
										<div class="col-sm-7">
		                                    
		                                        <select  id="pageDis"  >
																<option value="0">否</option>
																<option value="1">是</option>
													</select>
		                                </div>
									</div>
                		
                					<div class="modal-footer">
			                   	 	<button type="button"  class="btn btn-primary" id="btn_pageReset" onClick="pageReset();">重置</button>
			                        <button id="btn_pageSave" type="button" class="btn btn-primary" onclick="savePage()">保存</button>
			                    </form>
                		
               				</div>
						</div>
					</div>
				</div>
			</div>
        </div>
	</div>







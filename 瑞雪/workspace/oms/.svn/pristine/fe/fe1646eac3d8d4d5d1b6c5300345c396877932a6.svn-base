        <!-- 加载主题风格样式 -->
        <link type="text/css" rel="stylesheet" href="/Apollo/madmin/css/themes/style1/orange-blue.css" class="default-style" />
        <link type="text/css" rel="stylesheet" href="/Apollo/madmin/css/themes/style1/orange-blue.css" id="theme-change" class="style-change color-change" />
        <link type="text/css" rel="stylesheet" href="/Apollo/madmin/css/style-responsive.css" />
</head>
<body>
  <div class="page-content">
	        <div class="container-fluid">
				<div class="row">
                    <div class="col-lg-12">
				        <div class="portlet box portlet-blue" style="margin-bottom: 15px;">
				            <div class="portlet-header">
				                <div class="caption">组织列表</div>
                                <div class="actions">
                                	<a href="#" class="btn btn-xs btn-warning" data-toggle="" data-target="" onclick="addOrg()">
                                		<i class="fa fa-plus"></i>新建组织
                                	</a>&nbsp;
                                	<a href="#" class="btn btn-xs btn-warning" data-toggle="" data-target="" onclick="updateOrg()">
                                		<i class="fa fa-edit"></i>修改组织
                                	</a>&nbsp;
                                	<a href="#" class="btn btn-xs btn-danger btn-xs" data-toggle="" data-target="" onclick="delOrg()">
                                		<i class="fa fa-trash-o"></i>删除组织
                                	</a>&nbsp;
                                </div>
				            </div>
				            <div class="portlet-body">
								<div id="orgTree" style="height:94%;width:100%;"></div>
				            </div>
				        </div>
                    </div>
                </div>
	        </div>
	   <!-- ====================================== 窗口区 组织单元===================================== -->
        <div id="FrameUnitOrg" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" class="modal fade">
            <form id="userOrgDetailForm" role="form" action="" class="form-horizontal">
                <div class="modal-dialog">
                    <div class="modal-content" style="width: 980px; position: absolute; overflow: auto;">
                        <div class="modal-header modal-header-primary">
                            <button type="button" data-dismiss="modal" aria-hidden="true" class="close">x</button>
                            <h4 id="modal-header-primary-label" class="modal-title">新建机构单元</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-lg-12 col-xs-12 col-sm-12 col-md-12">
                                    <ul id="generalTab" class="nav nav-tabs ul-edit responsive">
                                        <li class="active"><a href="#alert-tab" data-toggle="tab">基本信息</a></li>
                                        <li ><a href="#note-tab" data-toggle="tab">人员信息</a></li>
                                    </ul>
                                    <div id="generalTabContent" class="tab-content responsive">
                                        <div id="alert-tab" class="tab-pane fade in active">
                                            <div class="form-group">
                                                <label for="orgName" class="col-sm-3 control-label">组织单元名称:<span class='require'>*</span>
                                                </label>
                                                <div class="col-sm-9">
                                                    <input id="sendunitId" type="hidden" /> <input id="sendflag" type="hidden" /> <input
                                                        required id="orgName" name="orgName" required type="text" placeholder="" maxlength="32"
                                                        class="form-control invalid" /> <input id="" type="hidden" /> <i
                                                        class="alert alert-hide">项目是必选项！</i>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="txtUserPassword" class="col-sm-3 control-label">描述信息: </label>
                                                <div class="col-sm-9">
                                                    <input id="orgDes" name="orgDes" type="text" placeholder="" maxlength="32"
                                                        class="form-control" />
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="maxPerson" class="col-sm-3 control-label">组织人员数: </label>
                                                <div class="col-sm-9">
                                                    <input id="maxPerson" name="maxPerson" type="text" value="-1" maxlength="12"
                                                        class="form-control" />
                                                         <input type="hidden" value="-1" id="maxPersonValue">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="orgSta" class="col-sm-3 control-label">上级领导岗位: </label>
                                                <div class="col-sm-9">
                                                    <div class="input-group">
                                                        <input type="text" id="orgSta" name="orgSta" placeholder="" class="form-control" required
                                                            disabled /> <input type="hidden" id="stationIdHidden"> <span
                                                            class="input-group-addon btn btn-primary" id="showSta" style="color: white;">...</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="txtUserDesc" class="col-sm-3 control-label">组织类型: </label>
                                                <div class="col-sm-9">
                                                    <select id="orgTypeSelect" name="" class="form-control">
                                                        <option value="">请选择</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="note-tab" class="tab-pane fade">
                                            <div class="form-group">
                                                <label for="orgType" class="col-sm-2 control-label">组织机构: </label>
                                                <div class="col-sm-4">
                                                    <div class="input-group">
                                                        <input id="orgType" name="orgType" type="text" class="form-control "/>
                                                        <input id="orgTypeValue" name="orgTypeValue" type="hidden" /> 
                                                        <span class="input-group-addon btn btn-primary" id="selectUnit" style="color: white;">...</span>
                                                    </div>
                                                </div>
                                                 <div class="col-sm-1"></div>
                                                <div class="col-sm-2">
                                                    <div class="input-group">
                                                        <span class="input-group-addon" id="moveLeftToRight" style="width: 100%">移入</span>
                                                    </div>
                                                </div>
                                                <div class="col-sm-2">
                                                    <div class="input-group">
                                                        <span class="input-group-addon" id="outPut" style="width: 100%">移出</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-1"></div>
                                            <div class="form-group">
                                                <div class="col-lg-12 col-xs-12 col-sm-12 col-md-12">
                                                    <div class="col-lg-6 col-xs-6 col-sm-6 col-md-6">
                                                        <div class="panel panel-yellow">
                                                            <div class="panel-heading">用户列表</div>
                                                            <div id="kbdy" class="panel-body">
                                                                <table id="selectOrgTable" class="table table-hover">
                                                                    <thead>
                                                                        <tr>
                                                                            <th width="15%" colValue=""></th>
                                                                            <th width="45%" colValue="userAccount">用户账号</th>
                                                                            <th width="40%" colValue="userFullname">用户姓名</th>
                                                                        </tr>
                                                                    </thead>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-xs-6 col-sm-6 col-md-6">
                                                        <div class="panel panel-yellow">
                                                            <div class="panel-heading">已选列表</div>
                                                            <div class="panel-body">
                                                                <table id="selectOrgTable2" class="table table-hover">
                                                                    <thead>
                                                                        <tr>
                                                                            <th width="15%" colValue=""></th>
                                                                            <th width="45%" colValue="userAccount">用户账号</th>
                                                                            <th width="40%" colValue="userFullname">用户姓名</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody id="newtbody"></tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer" style="border-top: 0px">
                            <button type="button" data-dismiss="modal" class="btn btn-default">关闭</button>
                            <button id="saveFormElement" type="button" class="btn btn-primary">保存</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
        <!-- ====================================== 窗口区 组织机构===================================== -->
		<div id="MyUnitSelect" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" class="modal fade">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header modal-header-primary">
						<button type="button" data-dismiss="modal" aria-hidden="true" class="close">x</button>
						<h4 id="modal-header-primary-label" class="modal-title">请选择组织</h4>
					</div>
					<div class="modal-body" >
	   					<iframe id="UnitFrame" src="../views/system/organization/unit/UnitSelect.html?check=false" frameborder="0" noResize width="100%" scrolling="auto" height="300px"  vspale="0"></iframe>
					</div>
					<div class="modal-footer">
						<button type="button" data-dismiss="modal" class="btn btn-default">关闭</button>
						<button id="unitSelectSave" type="button" class="btn btn-primary">保存</button>
					</div>
				</div>
			</div>
		</div>
	   <!-- ====================================== 窗口区 上级领导岗位===================================== -->
		<div id="manageSelect" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" class="modal fade">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header modal-header-primary">
						<button type="button" data-dismiss="modal" aria-hidden="true" class="close">x</button>
						<h4 id="modal-header-primary-label" class="modal-title">上级领导岗位</h4>
					</div>
					<div class="modal-body">
							<div>
	   							<iframe id="postionIframe" src="../views/system/organization/unit/StationSelect.html" frameborder="0" noResize width="100%"  height="300px"  vspale="0"></iframe>
							</div>
							<div class="modal-footer">
								<button type="button" data-dismiss="modal" class="btn btn-default">关闭</button>
								<button id="postionSave" type="button" class="btn btn-primary">保存</button>
							</div>
					</div>
				</div>
			</div>
		</div>
	
	<!--END CONTENT -->

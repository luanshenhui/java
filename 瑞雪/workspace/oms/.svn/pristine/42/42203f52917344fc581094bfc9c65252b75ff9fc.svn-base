public enum BaseResultEnum {
	INSIDE_ERROR(-4,"服务内部错误"),
	EXCEPTION(-1,"系统异常"),
	SUCCESS(0,"调用成功"),	
	;
	BaseResultEnum(Integer c,String s){
		this.c = c;
		this.s = s;
	}
	private final Integer c;
	private final String s;
	public Integer getC() {
		return c;
	}
	public String getS() {
		return s;
	}
}

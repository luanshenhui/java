<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
    version="2.0">
    <description>this is my tag library Definition</description>
    <tlib-version>1.0</tlib-version>
    <short-name>mytaglib</short-name>
    <uri>taglib</uri>
      <tag>
        <name>foreach</name>
        <tag-class>cn.upnock.share.tag.ForeachTag</tag-class>
        <body-content>scriptless</body-content>
       <!-- 指定foreach循环中的items属性和var属性 -->
        <attribute>
            <name>items</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>var</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>index</name>
            <required>false</required>
            <fragment>true</fragment>
        </attribute>
    </tag>
    <tag>
    	<name>choose</name>
        <tag-class>cn.upnock.share.tag.ChooseTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>isDo</name>
            <required>false</required>
            <fragment>true</fragment>
        </attribute>
    </tag>
    <tag>
        <name>if</name>
        <tag-class>cn.upnock.share.tag.WhenTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>test</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
    </tag>
    <tag>
        <name>else</name>
        <tag-class>cn.upnock.share.tag.ElseTag</tag-class>
        <body-content>scriptless</body-content>
    </tag>
    <tag>
        <name>outDate</name>
        <tag-class>cn.upnock.share.tag.OutDateTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>value</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>format</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
    </tag>
    <tag>
        <name>paging</name>
        <tag-class>cn.upnock.share.tag.Paging</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>count</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>pageSize</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>pageNo</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>action</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
    </tag>
</taglib>
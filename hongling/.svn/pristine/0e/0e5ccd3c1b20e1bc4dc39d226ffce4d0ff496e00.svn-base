<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib uri="/struts-tags" prefix="s"%>
<script src="<%=request.getContextPath() %>/pages/common/changepasswordjsp.js"></script>
<form id="form" class="form_template show">
<table>
	<tr>
		<td id="lblPassword" class="label"><s:text name="lblPassword"></s:text></td>
		<td><input name="password" id="password" type="password"/></td>
	</tr>
	<tr>
		<td id="lblNewPassword" class="label"><s:text name="lblNewPassword"></s:text></td>
		<td><input name="newPassword" id="newPassword" type="password"/></td>
	</tr>
	<tr>
		<td id="lblVerfyNewPassword" class="label"><s:text name="lblVerifyPassword"></s:text></td>
		<td><input name="verfyNewPassword" id="verfyNewPassword" type="password"/></td>
	</tr>
</table>
<div class="operation">
<a id="btnChangePassword"><s:text name="btnSubmitOrden"></s:text></a> <a id="btnCancelPassword"><s:text name="btnCancelOrden"></s:text></a>
</div>
</form>
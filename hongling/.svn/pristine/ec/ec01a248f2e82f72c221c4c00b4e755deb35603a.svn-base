package chinsoft.test;

import net.sf.json.JSONObject;
import chinsoft.bean.SingleSingOnBean;
import chinsoft.business.CDict;
import chinsoft.service.core.Encryption;
import flexjson.JSONSerializer;

public class TestGetXmlFromOrden {
	public static void main(String[] args) {
		SingleSingOnBean bean=new SingleSingOnBean();
		bean.setCustomerName("杨磊");
		bean.setLanguage("1");
		bean.setPassword("123");
		bean.setUsername("yl");
		bean.setCustomerID("111");
		bean.setCompany("RC000001");
		
		String json=new JSONSerializer().exclude("*.class").deepSerialize(bean);
		System.out.println(json);
		try {
			System.out.println("http://localhost/hongling/login?param="+Encryption.encrypt(json,CDict.DES_KEY));
			System.out.println(Encryption.decrypt("", CDict.DES_KEY));
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		JSONObject  js=JSONObject.fromObject(json);
		SingleSingOnBean bean2=(SingleSingOnBean) JSONObject.toBean(js,SingleSingOnBean.class);
		System.out.println(bean2.getCustomerName());
		
		
	}
}

<script src="../realmname/common.js"></script>
<script src="../realmname/list.js"></script>
<div id="RealmNameSearch" class="list_search">
	<h1></h1>
	<label class="lblKeyword"></label><input type="text" id="keyword"/>
	<a id="btnSearch"></a>
	<a id="btnRemoveRealm"></a>
	<a id="btnCreateRealm"></a>
</div>
<div id="RealmNameResult"></div>
<div id="RealmNameStatistic"></div>
<div id="RealmNamePagination" class="paging"></div>
<textarea  id="RealmNameTemplate" class="list_template">
	<table class="list_result">
		<tr class="header">
			<td class='check'><input type="checkbox" onclick="$.csControl.checkAll('chkRow', this.checked);"/></td>
			<td class="lblCustomerName"></td>
			<td class="lblRealmName"></td>
			<td class="lblTitle"></td>
			<td class="lblEdit"></td>
		</tr>
		{#foreach $T.data as row}
		<tr id="row{$T.row.ID}">
			<td class="center check"><input type="checkbox" value="{$T.row.ID}" name="chkRow"/></td>
			<td class="link" onclick="$.csRealmNameList.openView('{$T.row.ID}')">{$T.row.customerName}</td>
			<td>{$T.row.realmName}</td>
			<td>{$T.row.title}</td>
			<td class="center"><a class="edit" onclick="$.csRealmNameList.openPost('{$T.row.ID}')"></a></td>
		</tr>
		{#/for}
	</table>
</textarea >
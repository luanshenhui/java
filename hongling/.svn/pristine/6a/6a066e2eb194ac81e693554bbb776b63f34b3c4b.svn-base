package chinsoft.entity;

import java.util.List;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement(name="Order")
@XmlAccessorType(XmlAccessType.FIELD)
public class OrdenXMLBean {
	
	@XmlElement(name="OrderDate")
	private String orderDate;
	@XmlElement(name="CustomerOrdersNo")
	private String customerOrdersNo;
	@XmlElement(name="ContractSerialNumber")
	private String contractSerialNumber;
	@XmlElement(name="Createman")
	private String createman;
	
	@XmlElement(name="CustomerName")
	private String customerName;
	@XmlElement(name="OwnedPartner")
	private String ownedPartner;
	@XmlElement(name="OwnedStore")
	private String ownedStore; 
	@XmlElement(name="Height")
	private String height;
	@XmlElement(name="Weight")
	private String weight;
	@XmlElement(name="Gender")
	private String gender;
	
	@XmlElement(name="Fabrics")
	private String fabrics;
	
	@XmlElement(name="CustormerBody")
	private String custormerBody;
	@XmlElement(name="CustormerSize")
	private String custormerSize;
	
	@XmlElement(name="OperatorName")
	private String operatorName;
	
	@XmlElement(name = "UserOrdenNo")
	private String userordeno;//客户指定订单号
	
	@XmlElement(name = "Locking")
	private String locking;//订单锁定 --善融1，红领0
	
	@XmlElement(name="Product")
	private String product;//产品
	@XmlElement(name="Money")
	private String money;//产品金额
	@XmlElement(name="TelPhone")
	private String telPhone;// 电话
	
	@XmlElement(name="Details")
	private List<OrdenDetailXMLBean> details;
	@XmlElement(name="Logistic")
	private LogisticXMLBean logistic;//物流信息
	
	public String getProduct() {
		return product;
	}

	public void setProduct(String product) {
		this.product = product;
	}

	public String getMoney() {
		return money;
	}

	public void setMoney(String money) {
		this.money = money;
	}

	public String getTelPhone() {
		return telPhone;
	}

	public void setTelPhone(String telPhone) {
		this.telPhone = telPhone;
	}

	public List<OrdenDetailXMLBean> getDetails() {
		return details;
	}

	public void setDetails(List<OrdenDetailXMLBean> details) {
		this.details = details;
	}

	public String getOrderDate() {
		return orderDate;
	}

	public void setOrderDate(String orderDate) {
		this.orderDate = orderDate;
	}

	public String getCustomerOrdersNo() {
		return customerOrdersNo;
	}

	public void setCustomerOrdersNo(String customerOrdersNo) {
		this.customerOrdersNo = customerOrdersNo;
	}

	public String getContractSerialNumber() {
		return contractSerialNumber;
	}

	public void setContractSerialNumber(String contractSerialNumber) {
		this.contractSerialNumber = contractSerialNumber;
	}

	public String getCreateman() {
		return createman;
	}

	public void setCreateman(String createman) {
		this.createman = createman;
	}

	public String getCustomerName() {
		return customerName;
	}

	public void setCustomerName(String customerName) {
		this.customerName = customerName;
	}

	public String getOwnedPartner() {
		return ownedPartner;
	}

	public void setOwnedPartner(String ownedPartner) {
		this.ownedPartner = ownedPartner;
	}

	public String getOwnedStore() {
		return ownedStore;
	}

	public void setOwnedStore(String ownedStore) {
		this.ownedStore = ownedStore;
	}

	public String getHeight() {
		return height;
	}

	public void setHeight(String height) {
		this.height = height;
	}

	public String getWeight() {
		return weight;
	}

	public void setWeight(String weight) {
		this.weight = weight;
	}

	public String getFabrics() {
		return fabrics;
	}

	public void setFabrics(String fabrics) {
		this.fabrics = fabrics;
	}

	public String getCustormerBody() {
		return custormerBody;
	}

	public void setCustormerBody(String custormerBody) {
		this.custormerBody = custormerBody;
	}

	public String getCustormerSize() {
		return custormerSize;
	}

	public void setCustormerSize(String custormerSize) {
		this.custormerSize = custormerSize;
	}

	public String getOperatorName() {
		return operatorName;
	}

	public void setOperatorName(String operatorName) {
		this.operatorName = operatorName;
	}

	public String getUserordeno() {
		return userordeno;
	}

	public void setUserordeno(String userordeno) {
		this.userordeno = userordeno;
	}

	public String getLocking() {
		return locking;
	}

	public void setLocking(String locking) {
		this.locking = locking;
	}

	public LogisticXMLBean getLogistic() {
		return logistic;
	}

	public void setLogistic(LogisticXMLBean logistic) {
		this.logistic = logistic;
	}

	public String getGender() {
		return gender;
	}

	public void setGender(String gender) {
		this.gender = gender;
	}

}

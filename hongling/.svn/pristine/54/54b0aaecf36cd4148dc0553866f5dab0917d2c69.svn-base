<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="chinsoft.entity.Lapel" table="Lapel">
        <id name="ID">
            <column name="ID"/>
            <generator class="assigned" />
        </id>
        <property name="buttonID" type="integer" column="buttonID"/>
        <property name="lapelID" type="integer" column="lapelID"/>
        <property name="widthID" type="integer" column="widthID"/>
    </class>
</hibernate-mapping>

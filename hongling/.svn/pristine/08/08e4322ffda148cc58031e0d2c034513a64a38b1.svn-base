package chinsoft.entity;

import java.util.Date;

public class Cash implements java.io.Serializable {
	private static final long serialVersionUID = 5884511911359063074L;
	private String ID;
	private String memo;
	private Double num;
	private String pubMemberID;
	private Date pubDate;
	private Member member;
	private Integer isReconciliation;
	private String isReconciliationName;
	private Integer noticeNum;          // 账户提醒金额
	private Integer stopNum;            // 账户停用金额
	private Integer statusId;           // 状态
	
	public String getIsReconciliationName() {
		return isReconciliationName;
	}
	public void setIsReconciliationName(String isReconciliationName) {
		this.isReconciliationName = isReconciliationName;
	}
	public Integer getIsReconciliation() {
		return isReconciliation;
	}

	public void setIsReconciliation(Integer isReconciliation) {
		this.isReconciliation = isReconciliation;
	}

	public String getID() {
		return ID;
	}

	public void setID(String ID) {
		this.ID = ID;
	}

	public String getMemo() {
		return memo;
	}

	public void setMemo(String memo) {
		this.memo = memo;
	}

	public Double getNum() {
		return num;
	}

	public void setNum(Double num) {
		this.num = num;
	}

	public String getPubMemberID() {
		return this.pubMemberID;
	}

	public void setPubMemberID(String pubMemberID) {
		this.pubMemberID = pubMemberID;
	}

	public Date getPubDate() {
		return this.pubDate;
	}

	public void setPubDate(Date pubDate) {
		this.pubDate = pubDate;
	}

	public void setMember(Member member) {
		this.member = member;
	}

	public Member getMember() {
		return member;
	}

	public Integer getNoticeNum() {
		return noticeNum;
	}

	public void setNoticeNum(Integer noticeNum) {
		this.noticeNum = noticeNum;
	}

	public Integer getStopNum() {
		return stopNum;
	}

	public void setStopNum(Integer stopNum) {
		this.stopNum = stopNum;
	}
	public Integer getStatusId() {
		return statusId;
	}
	public void setStatusId(Integer statusId) {
		this.statusId = statusId;
	}
	
}
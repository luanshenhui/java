.topnav {
	width: 100%;
	padding: 0;
}

.topnav .label,.topnav .value{display:inline;}
.topnav .label{}
.topnav .value{width:210px;}

ul.topnav {
	padding: 0;
	margin: 0;
	list-style: none;
}

ul.topnav li {line-height:18px;}

ul.topnav li a {
	color: #000;
	display: block;
	text-decoration: none;
	font-weight: bolder;
}

ul.topnav li a:hover {
	background-color:#675C7C;
	color:white;
}
ul.topnav ul {
	margin: 0;
	padding: 0;
	display: none;
}
ul.topnav ul li {
	margin: 0;
	padding: 0;
	clear: both;
}
ul.topnav ul li a:hover {
	background-color:#D3C99C;
	color:#675C7C;
}
ul.topnav ul li a {
	padding-left: 30px;
	font-weight: normal;
	outline:0;
}

ul.topnav ul ul li a {
	color:silver;
	padding-left: 60px;
}

ul.topnav ul ul ul li a{
	color:silver;
	padding-left: 90px;
}

ul.topnav ul ul ul ul li a{
	color:silver;
	padding-left: 120px;
}

ul.topnav ul ul ul ul ul li a{
	color:silver;
	padding-left: 150px;
}

ul.topnav ul ul ul ul ul ul li a{
	color:silver;
	padding-left: 180px;
}

ul.topnav ul ul li a:hover {
	background-color:#D3CEB8;
	color:#675C7C;
}

ul.topnav span{
	float:right;
}
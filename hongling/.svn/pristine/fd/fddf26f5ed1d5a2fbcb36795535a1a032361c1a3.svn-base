<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib uri="/struts-tags" prefix="s"%>
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<title>RCMTM</title>
<link rel="shortcut icon" href="rcmtm.ico" /> 
<meta http-equiv='Content-Type' content='text/html;charset=utf-8' />
<meta http-equiv='pragma' content='no-cache' />
<meta http-equiv='cache-control' content='no-store' />
<meta http-equiv='expires' content='0' />
<script language= "javascript" src="../../scripts/jsp.js"></script>
<script type="text/javascript" src="ordenview.js"></script>
<style>
body {
	height: 100%;
	padding: 0;
	margin: 0;
    color: #FFFFFF;
    TEXT-ALIGN: center;
}
div#all {
	width: 1000px;
	height: 100%;
	margin:0px auto;
	
}
.label{
	 font-size: 14px;
    font-weight: bold;
    text-align: left;
}
.value{
	 font-size: 14px;
    text-align: left;
}
</style>
</head>
<body>
<div id="all">
<div id="view_orden" class="form_template">
	<table>
		<tr>
			<td colspan="6" class="caption lblBasicInfo"><s:text name="lblBasicInfo"></s:text></td>
		</tr>
		<tr>
			<td class="label lblCode"><s:text name="lblCode"></s:text></td>
			<td id="_view_ordenID"></td>
			<td class="label lblSysCode"><s:text name="lblSysCode"></s:text></td>
			<td id="_view_sysCode"></td>
			<td class="label lblMemberName"><s:text name="lblName"></s:text></td>
			<td id="_view_pubMemberName"></td>
		</tr>
		<tr>
			<td class="label lblPubDate"><s:text name="lblPubDate"></s:text></td>
			<td id="_view_ordenPubDate"></td>
			<td class="label lblStatusName"><s:text name="lblStatus"></s:text></td>
			<td id="_view_statusName"></td>
			<td class="label lblStyle"><s:text name="styleTitle"></s:text></td>
			<td id="_view_styleName"></td>
		</tr>
		<tr>
			<td colspan="6" class="caption lblCustomer"><s:text name="lblCustomer"></s:text></td>
		</tr>
		<tr>
			<td class="label lblCustomerNo"><s:text name="lblCustomerNo"></s:text></td>
			<td id="_view_userNo"></td>
			<td class="label lblName"><s:text name="Name"></s:text></td>
			<td id="_view_name"></td>
			<td class="label lblGender"><s:text name="lblGender"></s:text></td>
			<td id="_view_genderName"></td>
		</tr>
		<tr>
			<td class="label lblTel"><s:text name="lblTel"></s:text></td>
			<td id="_view_tel"></td>
			<td class="label lblEmail"><s:text name="lblEmail"></s:text></td>
			<td id="_view_email"></td>
			<td class="label lblWeight"><s:text name="lblWeight"></s:text></td>
			<td id="_view_weight"></td>
		</tr>
		<tr>
			<td class="label lblHeight"><s:text name="lblHeight"></s:text></td>
			<td id="_view_height"></td>
			<td class="label lblAddress"><s:text name="lblAddress"></s:text></td>
			<td id="_view_address"></td>
			<td class="label lblLtName"><s:text name="lblLtName"></s:text></td>
			<td id="_view_ltName"></td>
		</tr>
		<tr>
			<td class="label lblMemo"><s:text name="lblMemo"></s:text></td>
			<td id="_view_memos"></td>
		</tr>
		<tr>
			<td colspan="6" class="caption lblOrdenInfo"><s:text name="lblOrdenInfo"></s:text></td>
		</tr>
		<tr>
			<td class="label lblClothingCategory"><s:text name="lblClothingCategory"></s:text></td>
			<td id="_view_clothingName"></td>
			<td class="label lblFabric"><s:text name="lblFabric"></s:text></td>
			<td id="_view_fabricCode"></td>
			<td class="label"></td>
			<td ></td>
		</tr>
		<tr>
			<td class="label lblDetail"><s:text name="lblDetail"></s:text></td>
			<td colspan="5" id="_view_detail"></td>
		</tr>
		<tr>
			<td class="label lblEmbroidery"><s:text name="lblEmbroidery"></s:text></td>
			<td colspan="5" id="_view_embroidery"></td>
		</tr>
		<tr>
			<td colspan="6" class="caption lblSizeInfo"><s:text name="lblSizeInfo"></s:text></td>
		</tr>
		<tr>
			<td class="label lblSizeCategory"><s:text name="lblSizeCategory"></s:text></td>
			<td id="_view_sizeCategoryName"></td>
			<td class="label lblUnit"><s:text name="lblUnit"></s:text></td>
			<td id="_view_sizeUnitName"></td>
			<td id="lblClothingStyle" class="label lblClothingStyle"><s:text name="lblClothingStyle"></s:text></td>
			<td id="_view_clothingStyleName"></td>
		</tr>
		<tr>
			<td class="label lblSizePart"><s:text name="lblSizePart"></s:text></td>
			<td colspan="5" id="_view_sizePart"></td>
		</tr>
		<tr>
			<td id="lblBodyType" class="label lblBodyType"><s:text name="lblBodyType"></s:text></td>
			<td colspan="5" id="_view_bodyType"></td>
		</tr>
	</table> 
</div>
</div>
</body>
</html>
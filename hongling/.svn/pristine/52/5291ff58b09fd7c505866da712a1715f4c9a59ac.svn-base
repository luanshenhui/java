package hongling.entity;

import java.io.Serializable;
/**
 * 款式 设计组合表(分表)
 * 
 * @author Administrator
 * 
 */
public class StyleProcess implements Serializable {
	
	private static final long serialVersionUID = -1692249380547965520L;
	/** ID */
	private Integer ID;
	/** 代码 */
	private String styleCode;
	/** 面料 */
	private String fabricCode;
	/** 款式工艺ID */
	private String process;
	/** 特殊工艺ID:内容 */
	private String specialProcess;
	/** 款式工艺ecode */
	private String processCode;
	/** 特殊工艺ecode:内容 */
	private String specialProcessCode;
	/** 衬衣长短袖（长袖：3028，短袖：3029） */
	private Integer shirt;
	
	public Integer getID() {
		return ID;
	}
	public void setID(Integer iD) {
		ID = iD;
	}
	public String getStyleCode() {
		return styleCode;
	}
	public void setStyleCode(String styleCode) {
		this.styleCode = styleCode;
	}
	public String getFabricCode() {
		return fabricCode;
	}
	public void setFabricCode(String fabricCode) {
		this.fabricCode = fabricCode;
	}
	public String getProcess() {
		return process;
	}
	public void setProcess(String process) {
		this.process = process;
	}
	public String getSpecialProcess() {
		return specialProcess;
	}
	public void setSpecialProcess(String specialProcess) {
		this.specialProcess = specialProcess;
	}
	public String getProcessCode() {
		return processCode;
	}
	public void setProcessCode(String processCode) {
		this.processCode = processCode;
	}
	public String getSpecialProcessCode() {
		return specialProcessCode;
	}
	public void setSpecialProcessCode(String specialProcessCode) {
		this.specialProcessCode = specialProcessCode;
	}
	public Integer getShirt() {
		return shirt;
	}
	public void setShirt(Integer shirt) {
		this.shirt = shirt;
	}
	
}

#middle_title {
   /*  width: 1200px; */
    /* margin: 100px auto; */
	width: 1300px;
}

ul, li {
    margin: 0;
    padding: 0;
}

#blob {
	background: #f4f0f0;
    border-right: 1px solid #0059ec;
    border-left: 1px solid #0059ec;
    position: absolute;
    z-index: 1;
    top: 0;
   background: -moz-linear-gradient(top, #f4f0f0, #7a7777);
   background: -webkit-gradient(linear, left top, left bottom, from(#f4f0f0), to(#7a7777));
   filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#f4f0f0,endcolorstr=#7a7777,gradientType=0);
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    -moz-box-shadow: 2px 3px 10px #011331;
    -webkit-box-shadow: 2px 3px 10px #011331;
}

#nav {
    position: relative;
    background: #292929;
    float: left;
	margin-left: 450px;
    margin-top: 55px;
    position: absolute;
}

#nav li {
    float: left;
    list-style: none;
    border-right: 1px solid #4a4a4a;
    border-left: 1px solid black;
}

#nav li a {
    color: #e3e3e3;
    z-index: 2;
    position: relative;
    cursor: pointer;
    float: left;
	font-size: 24px;
    font-family:微软雅黑;
    text-decoration: none;
    padding: 20px 45px;
    width: 100%;

}

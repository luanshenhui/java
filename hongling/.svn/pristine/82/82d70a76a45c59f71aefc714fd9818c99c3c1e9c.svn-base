package centling.dto;

public class DealItemDto implements java.io.Serializable{
	private static final long serialVersionUID = 3337130850996234070L;
	private Integer ID;         // ID
	private String memo;        // 备注
	private String name;        // 交易项目名称
	private Integer ioFlag;     // 收入支出标志
	private String ioFlagName;  // 收入支出标志名
	private String En;			// 英文名称
	public DealItemDto() {
		super();
	}
	public DealItemDto(Integer iD, String memo, String name, Integer ioFlag,
			String ioFlagName, String en) {
		super();
		ID = iD;
		this.memo = memo;
		this.name = name;
		this.ioFlag = ioFlag;
		this.ioFlagName = ioFlagName;
		this.En = en;
	}
	
	
	public String getEn() {
		return En;
	}
	public void setEn(String en) {
		En = en;
	}
	public Integer getID() {
		return ID;
	}
	public void setID(Integer iD) {
		ID = iD;
	}
	public String getMemo() {
		return memo;
	}
	public void setMemo(String memo) {
		this.memo = memo;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getIoFlag() {
		return ioFlag;
	}
	public void setIoFlag(Integer ioFlag) {
		this.ioFlag = ioFlag;
	}
	public String getIoFlagName() {
		return ioFlagName;
	}
	public void setIoFlagName(String ioFlagName) {
		this.ioFlagName = ioFlagName;
	}
}

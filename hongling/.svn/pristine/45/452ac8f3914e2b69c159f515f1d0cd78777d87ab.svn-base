<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="rcmtm.entity.Geo" table="Geo">
    	<id name="id" type="integer">
            <column name="ID"/>
        </id>
        <property name="parentid" type="integer">
            <column name="PARENTID" />
        </property>
        <property name="name" type="string">
            <column name="NAME" />
        </property>
        <property name="shortening" type="string">
            <column name="SHORTENING" />
        </property>
        <property name="ipfrom" type="long">
            <column name="IPFROM" />
        </property>
        <property name="ipto" type="long">
            <column name="IPTO" />
        </property>
    </class>
</hibernate-mapping>

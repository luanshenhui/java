<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags" %>
<script src="../fabrictrader/post.js"></script>
<form id="form" class="form_template">
<input type="hidden" name="ID" id="ID"/>
<h1>添加品牌档案</h1>
<div style="margin-left: 20px;">
<table>
	<tr>
		<td class="label star">商户名称</td>
		<td><input type="text" id="traderName" class="textbox"  style="width:389px;"/></td>
	</tr>
	<tr>
		<td class="label star">推荐面料</td>
		<td><input type="text" id="recommendation" class="textbox"  style="width:389px;"/></td>
	</tr>
	<tr>
		<td class="label star">地址</td>
		<td><input type="text" id="address" class="textbox"  style="width:389px;"/></td>
	</tr>
	<tr>
		<td class="label ">电话</td>
		<td><input type="text" id="telephone" class="textbox"  style="width:389px;"/></td>
	</tr>
	<tr>
		<td class="label ">品牌描述</td>
		<td><textarea id="remark" style="height: 135px; width: 385px;resize:none;"></textarea></td>
	</tr>
</table>

<div class="operation">
	<a id="btnSave" href="javascript:void(0);">添加</a><a id="btnCancel" onclick="$.csCore.close();" >取消</a>
</div>
</div>
</form>

<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="centling.entity.FabricPrice" table="FabricPrice">
        <id name="ID" column="ID" type="string">
            <generator class="uuid.hex" />
        </id>
        <property name="FabricCode" type="string">
            <column name="fabricCode" />
        </property>
        <property name="AreaId" type="integer">
            <column name="AreaId" />
        </property>
        <property name="RmbPrice" type="double">
            <column name="RmbPrice" />
        </property>
        <property name="DollarPrice" type="double">
            <column name="DollarPrice" />
        </property>
    </class>
</hibernate-mapping>

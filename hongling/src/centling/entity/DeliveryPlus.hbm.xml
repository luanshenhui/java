<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="centling.entity.DeliveryPlus" table="DeliveryPlus">
        <id name="ID" type="string">
            <column name="ID" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="DeliveryID" type="string">
            <column name="DeliveryID" />
        </property>
        <property name="PubMemberID" type="string">
            <column name="PubMemberID" />
        </property>
        <property name="OrdenID" type="string">
            <column name="OrdenID" />
        </property>
        <property name="Amount" type="string">
            <column name="Amount" />
        </property>
        <property name="FabricComposition" type="string">
            <column name="FabricComposition"/>
        </property>
         <property name="StatusID" type="integer">
            <column name="StatusID"/>
        </property>
    </class>
</hibernate-mapping>
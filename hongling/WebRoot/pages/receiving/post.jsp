		<td class="label star lblOwnedStore nowrap"></td>
		<td><input type="text" id="ownedStore" class="textbox"/></td>
	</tr>
	<tr>
		<td class="label star lblCode nowrap"></td>
		<td><input type="text" id="ordenid" class="textbox"/></td>
	</tr>
	<tr>
		<td class="label lblClothingCategory nowrap"></td>
		<td><input type="text" id="clothingCategory" class="textbox"/></td>
	</tr>
	<tr>
		<td class="label lblName nowrap"></td>
		<td><input type="text" id="name" class="textbox"/></td>
	</tr>
	<tr>
		<td class="label lblTel nowrap"></td>
		<td><input type="text" id="tel" class="textbox"/></td>
	</tr>
	<tr>
		<td class="label lblMemo nowrap"></td>

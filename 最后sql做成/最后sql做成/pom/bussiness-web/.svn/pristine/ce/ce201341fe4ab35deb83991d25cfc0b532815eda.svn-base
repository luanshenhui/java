    <script type="text/javascript">
	
	$("#change-transitions").children().remove();
	
	jQuery.ajax( {
		url : "login/getProvinceList.action",
		type : "post",
		success : function(data) {
			for(var a=0;a<data.data.length;a++){
				var html="<div class='col-md-2'>"+
			    			"<div class='box-placeholder'>"+
			    			"<button data-toggle='dropdown' data-value="+data.data[a].provinceid+" class='btn btn-success btn-block'>"+data.data[a].province+"("+data.data[a].id+")"+"</button>"+
							"</div>"+
						  "</div>";
						 $("#change-transitions").append(html);		
			}
			$("button").click(function () {
				  var provinceid=this.getAttribute('data-value')
				  window.location.href='login/groupList.action?provinceid='+provinceid;
			});
		}
	});
    </script>

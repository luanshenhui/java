/*审核样式*/
#checkActionContent li {
	list-style: none
}

#checkActionContent em {
	font-style: normal
}

#checkActionContent ul {
	margin: 0px;
	padding: 0px;
	font-size: 12px;
	padding-bottom: 8px;
	margin-bottom: 8px;
	border-bottom: solid 1px #034885
}

#checkActionContent .title {
	background: url(../images/tc_dina.gif) repeat-x bottom;
	padding-bottom: 5px
}

#checkActionContent .user {
	color: #1671c0;
	font-weight: bold;
	margin-right: 5px
}

#checkActionContent .content {
	line-height: 21px;
	margin-top: 5px;
	color: #505050;
	padding-left: 20px
}

#checkActionContent .pass {
	color: #009819;
	font-weight: bold
}

#checkActionContent .refuse {
	color: #d10000;
	font-weight: bold
}

.detailViewClass {
	width: 100%;
}

.detailViewClass th {
	background-color: #efefef
}

.comboCity {
	width: 80px;
	float: left;
	text-align: center;
	margin-left: 10px;
	margin-top: 5px;
	cursor: pointer
}

.comboCitySelect {
	color: #ffffff;
	background-color: #000033
}

.icon-b {
	width: 16px;
	height: 19px;
	background: url(../images/icon-b.gif) no-repeat scroll 0 0;
	display: inline-block;
}


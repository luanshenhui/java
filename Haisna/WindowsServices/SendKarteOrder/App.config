<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <appSettings>
    <!-- サービス名 -->
    <add key="ServiceName" value="HainsSendKarteOrder"/>
    <!-- 表示名 -->
    <add key="ServiceDisplayName" value="HainsSendKarteOrder"/>
    <!-- 説明 -->
    <add key="ServiceDescription" value="電子チャート依頼"/>
    <!-- 依存するサービス（カンマ","で区切って複数のサービス名を指定できます） -->
    <add key="ServiceDependedOn" value=""/>
    <!-- 送信ホスト名 -->
    <add key="ServerHostName" value="127.0.0.1"/>
    <!-- 送信ポート番号 -->
    <add key="ServerPort" value="38700"/>
    <!-- ポーリング間隔（単位:ミリ秒） -->
    <add key="PollingInterval" value="1000"/>
    <!-- ソケット受信タイムアウト（単位:ミリ秒） -->
    <add key="SocketRecieveTimeout" value="20000"/>
    <!-- ソケット送信タイムアウト（単位:ミリ秒） -->
    <add key="SocketSendTimeout" value="20000"/>
    <!-- 接続リトライ回数 -->
    <add key="ConnetctRetryCount" value="2"/>
    <!-- 送信リトライ回数 -->
    <add key="SendRetryCount" value="2"/>    
    <!-- 発信元コード -->
    <add key="FromCode" value="K"/>
    <!-- 端末名 -->
    <add key="SendDataWsMei" value="lsvwhdbs"/>
    <!-- 利用者ID -->
    <add key="SendDataRiyosyaId" value="097187"/>
    <!-- 利用者氏名 -->
    <add key="SendDataRiyosyaName" value="河北　俊子"/>
    <!-- 胸部Ｘ線撮影方法判定用 -->
    <add key="KyobuItemCode" value="21090"/>
    <!-- XML部テンプレートファイル名 -->
    <add key="XmlTemplateFileName" value="SendKarteOrderXmlTemplate.xml"/>
    <!-- Web API のベースURL -->
    <add key="ApiUri" value="http://localhost:52250/"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1"/>
  </startup>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target xsi:type="File" name="InfoLog" fileName="C:/Hainsi/WindowsServices/Logs/HainsSendKarteOrder/info.log" layout="${longdate} [${uppercase:${level:padding=-5}}] ${message} ${exception:format=tostring}" archiveEvery="Day" archiveFileName="C:/Hainsi/WindowsServices/Logs/HainsSendKarteOrder/info_{#}.log" archiveNumbering="Date" archiveDateFormat="yyyyMMdd" maxArchiveFiles="30"/>
      <target xsi:type="File" name="ErrorLog" fileName="C:/Hainsi/WindowsServices/Logs/HainsSendKarteOrder/error.log" layout="${longdate} [${uppercase:${level:padding=-5}}] ${message} ${exception:format=tostring}" archiveEvery="Day" archiveFileName="C:/Hainsi/WindowsServices/Logs/HainsSendKarteOrder/error_{#}.log" archiveNumbering="Date" archiveDateFormat="yyyyMMdd" maxArchiveFiles="30"/>
    </targets>
    <rules>
      <logger name="*" levels="Info,Error" writeTo="InfoLog"/>
      <logger name="*" levels="Error" writeTo="ErrorLog"/>
    </rules>
  </nlog>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>

<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <appSettings>
    <!-- サービス名 -->
    <add key="ServiceName" value="HainsGetPersonalProfile" />
    <!-- 表示名 -->
    <add key="ServiceDisplayName" value="HainsGetPersonalProfile" />
    <!-- 説明 -->
    <add key="ServiceDescription" value="個人プロファイル情報取得" />
    <!-- 依存するサービス（カンマ","で区切って複数のサービス名を指定できます） -->
    <add key="ServiceDependedOn" value="" />
    <!-- 送信ホスト名 -->
    <add key="ServerHostName" value="127.0.0.1" />
    <!-- 送信ポート番号 -->
    <add key="ServerPort" value="15110" />
    <!-- ポーリング間隔（単位:ミリ秒） -->
    <add key="PollingInterval" value="15000" />
    <!-- ソケット受信タイムアウト（単位:ミリ秒） -->
    <add key="SocketRecieveTimeout" value="20000" />
    <!-- ソケット送信タイムアウト（単位:ミリ秒） -->
    <add key="SocketSendTimeout" value="20000" />
    <!-- 端末名 -->
    <add key="SendDataWsMei" value="HAINSSVR" />
    <!-- 利用者ID -->
    <add key="SendDataRiyosyaId" value="097187" />
    <!-- Web API のベースURL -->
    <add key="ApiUri" value="http://localhost:50681/" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
  </startup>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target xsi:type="File" name="InfoLog" fileName="C:/Hainsi/WindowsServices/Logs/HainsGetPersonalProfile/info.log" layout="${longdate} [${uppercase:${level:padding=-5}}] ${message} ${exception:format=tostring}" archiveEvery="Day" archiveFileName="C:/Hainsi/WindowsServices/Logs/HainsGetPersonalProfile/info_{#}.log" archiveNumbering="Date" archiveDateFormat="yyyyMMdd" maxArchiveFiles="30" />
      <target xsi:type="File" name="ErrorLog" fileName="C:/Hainsi/WindowsServices/Logs/HainsGetPersonalProfile/error.log" layout="${longdate} [${uppercase:${level:padding=-5}}] ${message} ${exception:format=tostring}" archiveEvery="Day" archiveFileName="C:/Hainsi/WindowsServices/Logs/HainsGetPersonalProfile/error_{#}.log" archiveNumbering="Date" archiveDateFormat="yyyyMMdd" maxArchiveFiles="30" />
    </targets>
    <rules>
      <logger name="*" levels="Info,Error" writeTo="InfoLog" />
      <logger name="*" levels="Error" writeTo="ErrorLog" />
    </rules>
  </nlog>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>

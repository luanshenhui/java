Function convertAddress(strKeyName)

	'連携先サーバ名格納用
	Dim strAddress

	'パラメタの識別IDから、連携先サーバ名を判断
	Select Case Trim(strKeyName)

		'誘導支援サーバ
		Case "Guide":
			strAddress = "lsvwhgui"

		'心電図サーバ
		Case "Ecg":
			strAddress = "157.104.34.11"

		'放射線サーバ（画像）
		Case "XrayImage":
	         	strAddress = "157.104.60.22"
'###			strAddress = "157.104.60.62"
'###			strAddress = "CWEB2.smile.net"

'#### 2011.07.01 SL-SN-Y0101-305 ADD START ####
		'福田電子サーバ
		Case "CaviABi":
			strAddress = "157.104.4.200"
'#### 2011.07.01 SL-SN-Y0101-305 ADD END ####

	End Select

	'連携先サーバ名を返却
	convertAddress = strAddress

End Function

